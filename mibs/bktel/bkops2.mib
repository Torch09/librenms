--*****************************************************************************************
--  ops2Control group

    ops2ControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ops2ControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ops2Control 1 }

-- table of control entry(ro/rw)

    ops2ControlEntry OBJECT-TYPE
        SYNTAX Ops2ControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { ops2NESlot }
    ::= { ops2ControlTable 1 }

-- the structure of the entry

    Ops2ControlEntry ::= SEQUENCE
    {
        ops2ControlReset            TruthValue,
        ops2ControlModuleLedBlink   TruthValue
    }

    ops2ControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { ops2ControlEntry 1 }

    ops2ControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { ops2ControlEntry 2 }

--*****************************************************************************************
-- ops2Configuration group

    ops2ConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ops2ConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ops2Configuration 1 }

-- table of configuration entry(ro/rw)

    ops2ConfigurationEntry OBJECT-TYPE
        SYNTAX Ops2ConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ops2NESlot  }
    ::= { ops2ConfigurationTable 1 }

-- the structure of the entry

    Ops2ConfigurationEntry ::= SEQUENCE
    {
        ops2ConfigNESlotWrite             NESlotWriteValue,

        ops2ConfigSwitchModeSwUnit1       Ops2SwitchModeValue,
        ops2ConfigSwitchModeSwUnit2       Ops2SwitchModeValue,
        ops2ConfigSwitchPositionSwUnit1   Ops2SwitchPositionValue,
        ops2ConfigSwitchPositionSwUnit2   Ops2SwitchPositionValue,
        ops2ConfigDetectionTypeSwUnit1    Ops2DetectionTypeValue,
        ops2ConfigDetectionTypeSwUnit2    Ops2DetectionTypeValue,
        ops2ConfigSwitchSpeedSwUnit1      Ops2SwitchSpeedValue,
        ops2ConfigSwitchSpeedSwUnit2      Ops2SwitchSpeedValue,
        ops2ConfigFallbackDelaySwUnit1    Ops2FallbackDelayValue,
        ops2ConfigFallbackDelaySwUnit2    Ops2FallbackDelayValue,
        ops2ConfigTestmodeAccess          Ops2TestmodeAccessValue,
        ops2ConfigTestpointMode           Ops2TestpointModeValue
    }

    ops2ConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
   ::= { ops2ConfigurationEntry 1 }

    ops2ConfigSwitchModeSwUnit1 OBJECT-TYPE
        SYNTAX Ops2SwitchModeValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch mode. (Switch unit #1)
                      manual(1),
                      automaticRevertive(2),
                      automaticNonRevertive(3)
                    "
    ::= { ops2ConfigurationEntry 2 }

    ops2ConfigSwitchModeSwUnit2 OBJECT-TYPE
        SYNTAX Ops2SwitchModeValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch mode. (Switch unit #2)
                      manual(1),
                      automaticRevertive(2),
                      automaticNonRevertive(3)
                    "
    ::= { ops2ConfigurationEntry 3 }

    ops2ConfigSwitchPositionSwUnit1 OBJECT-TYPE
        SYNTAX Ops2SwitchPositionValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch position. (Switch unit #1)
                      nominalPosition(1),
                      redundantPosition(2)
                    "
    ::= { ops2ConfigurationEntry 4 }

    ops2ConfigSwitchPositionSwUnit2 OBJECT-TYPE
        SYNTAX Ops2SwitchPositionValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch position. (Switch unit #2)
                      nominalPosition(1),
                      redundantPosition(2)
                    "
    ::= { ops2ConfigurationEntry 5 }

    ops2ConfigDetectionTypeSwUnit1 OBJECT-TYPE
        SYNTAX Ops2DetectionTypeValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch detection type. (Switch unit #1)
                      inputpowerOnly(1),
                      rfOmiAndInputpower(2)
                    "
    ::= { ops2ConfigurationEntry 6 }

    ops2ConfigDetectionTypeSwUnit2 OBJECT-TYPE
        SYNTAX Ops2DetectionTypeValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch detection type. (Switch unit #2)
                      inputpowerOnly(1),
                      rfOmiAndInputpower(2)
                    "
    ::= { ops2ConfigurationEntry 7 }

    ops2ConfigSwitchSpeedSwUnit1 OBJECT-TYPE
        SYNTAX Ops2SwitchSpeedValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch speed control. (Switch unit #1)
                      fast(1),   -> The switch unit will switch as fast as possible when input level drops below alarm limit
                      regular(2) -> The switch unit is moderately fast and will only switch after a slight delay to avoid unnecessary switching
                    "
    ::= { ops2ConfigurationEntry 8 }

    ops2ConfigSwitchSpeedSwUnit2 OBJECT-TYPE
        SYNTAX Ops2SwitchSpeedValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch speed control. (Switch unit #2)
                      fast(1),   -> The switch unit will switch as fast as possible when input level drops below alarm limit
                      regular(2) -> The switch unit is moderately fast and will only switch after a slight delay to avoid unnecessary switching
                    "
    ::= { ops2ConfigurationEntry 9 }

    ops2ConfigFallbackDelaySwUnit1 OBJECT-TYPE
        SYNTAX Ops2FallbackDelayValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch fallback delay in 'automatic revertive' mode in seconds. (Switch unit #1)"
    ::= { ops2ConfigurationEntry 10 }

    ops2ConfigFallbackDelaySwUnit2 OBJECT-TYPE
        SYNTAX Ops2FallbackDelayValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Switch fallback delay in 'automatic revertive' mode in seconds. (Switch unit #2)"
    ::= { ops2ConfigurationEntry 11 }

    ops2ConfigTestmodeAccess OBJECT-TYPE
        SYNTAX Ops2TestmodeAccessValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Testmode button access mode.
                      testmodeAllowed(1),
                      testmodeBlocked(2)
                    "
    ::= { ops2ConfigurationEntry 12 }

    ops2ConfigTestpointMode OBJECT-TYPE
        SYNTAX Ops2TestpointModeValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Testpoint configuration. (Switch unit #1)
                      activeChannel(1), -> TP1 will be connected to the active channel of switch unit #1
                                           TP2 will be connected to the active channel of switch unit #2
                      fixed(2)          -> TP1 will be connected to switch unit #1 nominal channel
                                           TP2 will be connected to switch unit #1 redundant channel
                                           Switch unit #2 is not connected to a TP in this mode
                    "
    ::= { ops2ConfigurationEntry 13 }


--*****************************************************************************************
-- Display group

    ops2DisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ops2DisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ops2Display 1 }

-- table of configuration entry(ro/rw)

    ops2DisplayEntry OBJECT-TYPE
        SYNTAX Ops2DisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ops2NESlot  }
    ::= { ops2DisplayTable 1 }

-- the structure of the entry

    Ops2DisplayEntry ::= SEQUENCE
    {
        ops2SecondSwitchUnitAvailable          TruthValue,
        ops2RfOmiDetectionSupported            TruthValue,
        ops2CurrentSwitchPositionSwUnit1       Ops2SwitchPositionValue,
        ops2CurrentSwitchPositionSwUnit2       Ops2SwitchPositionValue,

        ops2DisplayInternalVoltage1Supported   TruthValue,
        ops2DisplayInternalVoltage2Supported   TruthValue,

        ops2DisplayInputVoltageNominal         INTEGER,
        ops2DisplayInternalVoltage1Nominal     INTEGER,
        ops2DisplayInternalVoltage2Nominal     INTEGER
    }

    ops2SecondSwitchUnitAvailable OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if a second optical switch is available in this devices"
    ::={ ops2DisplayEntry 1 }

    ops2RfOmiDetectionSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if switch supports RF OMI detection"
    ::={ ops2DisplayEntry 2 }

    ops2CurrentSwitchPositionSwUnit1 OBJECT-TYPE
        SYNTAX  Ops2SwitchPositionValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The current switch position (Switch unit #1)"
    ::={ ops2DisplayEntry 3 }

    ops2CurrentSwitchPositionSwUnit2 OBJECT-TYPE
        SYNTAX  Ops2SwitchPositionValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The current switch position (Switch unit #2)"
    ::={ ops2DisplayEntry 4 }

-- *****

    ops2DisplayInternalVoltage1Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  optional
        DESCRIPTION "Reports true(1) if internal voltage no. 1 is supported"
    ::={ ops2DisplayEntry 5 }

    ops2DisplayInternalVoltage2Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  optional
        DESCRIPTION "Reports true(1) if internal voltage no. 2 is supported"
    ::={ ops2DisplayEntry 6 }

-- *****

    ops2DisplayInputVoltageNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  optional
        DESCRIPTION "The nominal input voltage"
    ::={ ops2DisplayEntry 7 }

    ops2DisplayInternalVoltage1Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  optional
        DESCRIPTION "The nominal internal voltage no. 1"
    ::={ ops2DisplayEntry 8 }

    ops2DisplayInternalVoltage2Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  optional
        DESCRIPTION "The nominal internal voltage no. 2"
    ::={ ops2DisplayEntry 9 }



