--  ops2States group

-- Note: For every control OId there must be a correspondant alarm OId!

    ops2StatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ops2StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ops2States 1 }

-- table of states entry(ro/rw)

    ops2StatesEntry OBJECT-TYPE
        SYNTAX Ops2StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { ops2NESlot }
    ::= { ops2StatesTable 1 }

-- the structure of the entry

    Ops2StatesEntry ::= SEQUENCE
    {
        ops2StatesBootloader                      PerceivedSeverityValue,
        ops2StatesCommLoss                        PerceivedSeverityValue,
        ops2StatesTemperatureLow                  PerceivedSeverityValue,
        ops2StatesTemperatureHigh                 PerceivedSeverityValue,
        ops2StatesInputVoltageLow                 PerceivedSeverityValue,
        ops2StatesInputVoltageHigh                PerceivedSeverityValue,
        ops2StatesInternalVoltage                 PerceivedSeverityValue,
        ops2StatesInputPowerNominalSwUnit1Low     PerceivedSeverityValue,
        ops2StatesInputPowerNominalSwUnit2Low     PerceivedSeverityValue,
        ops2StatesInputPowerNominalSwUnit1High    PerceivedSeverityValue,
        ops2StatesInputPowerNominalSwUnit2High    PerceivedSeverityValue,
        ops2StatesInputPowerRedundantSwUnit1Low   PerceivedSeverityValue,
        ops2StatesInputPowerRedundantSwUnit2Low   PerceivedSeverityValue,
        ops2StatesInputPowerRedundantSwUnit1High  PerceivedSeverityValue,
        ops2StatesInputPowerRedundantSwUnit2High  PerceivedSeverityValue,
        ops2StatesRfOmiNominalSwUnit1Low          PerceivedSeverityValue,
