BKTEL-HFC862-OPS2-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        OBJECT-TYPE
                 FROM RFC-1212
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM BKTEL-HFC862-BASE-MIB
    ;

    ops2                  OBJECT IDENTIFIER ::= { modules 121 }
    ops2Common            OBJECT IDENTIFIER ::= { ops2 1 }
    ops2States            OBJECT IDENTIFIER ::= { ops2 2 }
    ops2Configuration     OBJECT IDENTIFIER ::= { ops2 3 }
    ops2Control           OBJECT IDENTIFIER ::= { ops2 4 }
    ops2MeasuringValues   OBJECT IDENTIFIER ::= { ops2 5 }
    ops2Display           OBJECT IDENTIFIER ::= { ops2 6 }


--
-- Type definitions
--

    Ops2SwitchModeValue ::= INTEGER
    {
        manual(1),
        automaticRevertive(2),
        automaticNonRevertive(3)
    }

    Ops2SwitchPositionValue ::= INTEGER
    {
        nominalPosition(1),
        redundantPosition(2)
    }

    Ops2DetectionTypeValue ::= INTEGER
    {
        inputpowerOnly(1),
        rfOmiAndInputpower(2)
    }

    Ops2TestmodeAccessValue ::= INTEGER
    {
        testmodeAllowed(1),
        testmodeBlocked(2)
    }

    Ops2SwitchSpeedValue ::= INTEGER
    {
        fast(1),
        regular(2)
    }

    Ops2TestpointModeValue ::= INTEGER
    {
        activeChannel(1),
        fixed(2)
    }

    Ops2FallbackDelayValue ::= INTEGER (5..300)

    NESlotWriteValue ::= INTEGER (-1..99)


-- MibCompiler defs --
-- $idx$: InternalVoltage
-- $idx$: SwUnit
-- MibCompiler defs --


--*****************************************************************************************
-- OPS2 ( Dual optical switch )
--*****************************************************************************************

-- ops2Common group

    ops2CommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { ops2Common 1 }

    ops2CommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Ops2CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OPS2-type in the NE"
    ::= { ops2Common 2 }

-- table of states entry(ro/rw)

    ops2CommonEntry OBJECT-TYPE
        SYNTAX Ops2CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { ops2NESlot }
    ::= { ops2CommonTable 1 }

-- the structure of the entry

    Ops2CommonEntry ::= SEQUENCE
    {
        ops2NESlot                      NESlotValue,
        ops2CommonType                  DisplayString,
        ops2CommonDescr                 DisplayString,
        ops2CommonFirmwareId            DisplayString,
        ops2CommonModuleWidth           ModuleWidthValue
    }

    ops2NESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { ops2CommonEntry 1 }

    ops2CommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { ops2CommonEntry 2 }

    ops2CommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { ops2CommonEntry 3 }

    ops2CommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { ops2CommonEntry 4 }

    ops2CommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION
        "The width of the module in multiples of slots (1, 2, ...)
        Returns 0 for special housings (for example 19inch, etc...)"
    ::= { ops2CommonEntry 5 }

