--*****************************************************************************************
-- RMTS ( Dual direct modulated optical transmitter )
--*****************************************************************************************

-- rmtsCommon group

    rmtsCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { rmtsCommon 1 }

    rmtsCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RmtsCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the RMTS-type in the NE"
    ::= { rmtsCommon 2 }

-- table of states entry(ro/rw)

    rmtsCommonEntry OBJECT-TYPE
        SYNTAX RmtsCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { rmtsNESlot }
    ::= { rmtsCommonTable 1 }

-- the structure of the entry

    RmtsCommonEntry ::= SEQUENCE
    {
        rmtsNESlot                      NESlotValue,
        rmtsCommonType                  DisplayString,
        rmtsCommonDescr                 DisplayString,
        rmtsCommonFirmwareId            DisplayString,
        rmtsCommonModuleWidth           ModuleWidthValue
    }

    rmtsNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { rmtsCommonEntry 1 }

    rmtsCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { rmtsCommonEntry 2 }

    rmtsCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { rmtsCommonEntry 3 }

    rmtsCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { rmtsCommonEntry 4 }

    rmtsCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION
        "The width of the module in multiples of slots (1, 2, ...)
        Returns 0 for special housings (for example 19inch, etc...)"
    ::= { rmtsCommonEntry 5 }

