BKTEL-HFC862-RMTS-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    rmts                  OBJECT IDENTIFIER ::= { modules 122 }
    rmtsCommon            OBJECT IDENTIFIER ::= { rmts 1 }
    rmtsStates            OBJECT IDENTIFIER ::= { rmts 2 }
    rmtsConfiguration     OBJECT IDENTIFIER ::= { rmts 3 }
    rmtsControl           OBJECT IDENTIFIER ::= { rmts 4 }
    rmtsMeasuringValues   OBJECT IDENTIFIER ::= { rmts 5 }
    rmtsDisplay           OBJECT IDENTIFIER ::= { rmts 6 }


--
-- Type definitions
--

    InputOutputSetupValue ::= INTEGER
    {
        fourRfInputs(1),
        twoRfInputsTwoRfOutputs(2)
    }

    NESlotWriteValue ::= INTEGER (-1..99)


