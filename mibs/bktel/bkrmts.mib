--*****************************************************************************************
--  rmtsStates group

-- Note: For every control OId there must be a correspondant alarm OId!

    rmtsStatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RmtsStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { rmtsStates 1 }

-- table of states entry(ro/rw)

    rmtsStatesEntry OBJECT-TYPE
        SYNTAX RmtsStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { rmtsNESlot }
    ::= { rmtsStatesTable 1 }

-- the structure of the entry

    RmtsStatesEntry ::= SEQUENCE
    {
        rmtsStatesBootloader                   PerceivedSeverityValue,
        rmtsStatesCommLoss                     PerceivedSeverityValue,
        rmtsStatesTemperatureLow               PerceivedSeverityValue,
        rmtsStatesTemperatureHigh              PerceivedSeverityValue,
        rmtsStatesInputVoltageLow              PerceivedSeverityValue,
        rmtsStatesInputVoltageHigh             PerceivedSeverityValue,
        rmtsStatesInternalVoltage              PerceivedSeverityValue
    }

    rmtsStatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The device is running in bootloader mode
             without a legal application software."
    ::= { rmtsStatesEntry 1 }

    rmtsStatesCommLoss OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device.
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not
             by the device"
    ::= { rmtsStatesEntry 2 }

    rmtsStatesTemperatureLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature low"
    ::= { rmtsStatesEntry 3 }

    rmtsStatesTemperatureHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature high"
    ::= { rmtsStatesEntry 4 }

    rmtsStatesInputVoltageLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage low"
    ::= { rmtsStatesEntry 5 }

    rmtsStatesInputVoltageHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage high"
    ::= { rmtsStatesEntry 6 }

    rmtsStatesInternalVoltage OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Internal voltage high or low"
    ::= { rmtsStatesEntry 7 }


--*****************************************************************************************
--  rmtsControl group

    rmtsControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RmtsControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { rmtsControl 1 }

-- table of control entry(ro/rw)

    rmtsControlEntry OBJECT-TYPE
        SYNTAX RmtsControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { rmtsNESlot }
    ::= { rmtsControlTable 1 }

-- the structure of the entry

    RmtsControlEntry ::= SEQUENCE
    {
        rmtsControlReset            TruthValue,
        rmtsControlModuleLedBlink   TruthValue
    }

    rmtsControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { rmtsControlEntry 1 }

    rmtsControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { rmtsControlEntry 2 }

--*****************************************************************************************
-- rmtsConfiguration group

    rmtsConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RmtsConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { rmtsConfiguration 1 }

-- table of configuration entry(ro/rw)

    rmtsConfigurationEntry OBJECT-TYPE
        SYNTAX RmtsConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { rmtsNESlot  }
    ::= { rmtsConfigurationTable 1 }

-- the structure of the entry

    RmtsConfigurationEntry ::= SEQUENCE
    {
        rmtsConfigNESlotWrite             NESlotWriteValue,

        rmtsConfigInputOutputSetup        INTEGER,
        rmtsConfigRfOutputlevel           INTEGER
    }

    rmtsConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { rmtsConfigurationEntry 1 }

    rmtsConfigInputOutputSetup OBJECT-TYPE
        SYNTAX INTEGER
        {
            fourRfInputs(1),
            twoRfInputsTwoRfOutputs(2)
        }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF input and output configuration"
    ::= { rmtsConfigurationEntry 2 }

    rmtsConfigRfOutputlevel OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF output level in units of 0.1 dBuV
            only valid if rmtsDisplayAdditionalParamsSupported == true(1)"
    ::= { rmtsConfigurationEntry 3 }


--*****************************************************************************************
-- Display group

    rmtsDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF RmtsDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { rmtsDisplay 1 }

-- table of configuration entry(ro/rw)

    rmtsDisplayEntry OBJECT-TYPE
        SYNTAX RmtsDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { rmtsNESlot  }
    ::= { rmtsDisplayTable 1 }

-- the structure of the entry

    RmtsDisplayEntry ::= SEQUENCE
    {
        rmtsOpticsAvailable                    TruthValue,

        rmtsDisplayInternalVoltage1Supported   TruthValue,
        rmtsDisplayInternalVoltage2Supported   TruthValue,
        rmtsDisplayInternalVoltage3Supported   TruthValue,
        rmtsDisplayInternalVoltage4Supported   TruthValue,

        rmtsDisplayInputVoltageNominal         INTEGER,
        rmtsDisplayInternalVoltage1Nominal     INTEGER,
        rmtsDisplayInternalVoltage2Nominal     INTEGER,
        rmtsDisplayInternalVoltage3Nominal     INTEGER,
        rmtsDisplayInternalVoltage4Nominal     INTEGER,

        rmtsDisplayAdditionalParamsSupported   TruthValue,
        rmtsDisplayFskTransceiverFrequency     INTEGER
    }

    rmtsOpticsAvailable OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if the RMTS includes an optical receiver
                     Currently always reports false(2)"
    ::={ rmtsDisplayEntry 1 }

-- *****

    rmtsDisplayInternalVoltage1Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if internal voltage no. 1 is supported"
    ::={ rmtsDisplayEntry 2 }

    rmtsDisplayInternalVoltage2Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if internal voltage no. 2 is supported"
    ::={ rmtsDisplayEntry 3 }

    rmtsDisplayInternalVoltage3Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if internal voltage no. 3 is supported"
    ::={ rmtsDisplayEntry 4 }

    rmtsDisplayInternalVoltage4Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Reports true(1) if internal voltage no. 4 is supported"
    ::={ rmtsDisplayEntry 5 }

-- *****

    rmtsDisplayInputVoltageNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal input voltage"
    ::={ rmtsDisplayEntry 6 }

    rmtsDisplayInternalVoltage1Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal internal voltage no. 1"
    ::={ rmtsDisplayEntry 7 }

    rmtsDisplayInternalVoltage2Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal internal voltage no. 2"
    ::={ rmtsDisplayEntry 8 }

    rmtsDisplayInternalVoltage3Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal internal voltage no. 3"
    ::={ rmtsDisplayEntry 9 }

    rmtsDisplayInternalVoltage4Nominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal internal voltage no. 4"
    ::={ rmtsDisplayEntry 10 }

-- *****

    rmtsDisplayAdditionalParamsSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION 
            "Reports true(1) if the following variables are supported:
                rmtsDisplayFskTransceiverFrequency,
                rmtsConfigRfOutputlevel
            "
    ::={ rmtsDisplayEntry 11 }

    rmtsDisplayFskTransceiverFrequency OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The fsk tranceiver frequency in units of 0.01 MHz
            only valid if rmtsDisplayAdditionalParamsSupported == true(1)"
    ::={ rmtsDisplayEntry 12 }


END

