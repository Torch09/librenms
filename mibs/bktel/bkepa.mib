BKTEL-HFC862-EPA-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    epa                  OBJECT IDENTIFIER ::= { modules 110}
    epaCommon            OBJECT IDENTIFIER ::= { epa 1 }
    epaStates            OBJECT IDENTIFIER ::= { epa 2 }
    epaConfiguration     OBJECT IDENTIFIER ::= { epa 3 }
    epaControl           OBJECT IDENTIFIER ::= { epa 4 }
    epaMeasuringValues   OBJECT IDENTIFIER ::= { epa 5 }
    epaDisplay           OBJECT IDENTIFIER ::= { epa 6 }


--
-- Type definitions
--

        AgcMode ::= INTEGER
            {
            modeAgcOff(1),
            modeAgcOn(2)
            }

        RfMode ::= INTEGER
            {
            modeNormal(1),
            modeLowNoise(2),
            modeLowDistortion(3)
            }

        RedundancyMode ::= INTEGER
            {
            modeNoRedundancy(1),
            modeNominalDevice(2),
            modeRedundantDevice(3),
            modeAutoDetectBySlot(4)
            }

        DeviceActivityMode ::= INTEGER
            {
            forceStandby(1),
            alwaysActive(2),
            automatic(3)
            }

        DeviceActivityState ::= INTEGER
            {
            standby(1),
            active(2)
            }

        DeviceRedundancyState ::= INTEGER
            {
            nominalDevice(1),
            redundantDevice(2)
            }

        NESlotWriteValue ::= INTEGER (-1..99)


--*****************************************************************************************
-- EPA ( Optical Transmitter with design release V01 )

--*****************************************************************************************
-- epaCommon group

    epaCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { epaCommon 1 }

    epaCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EpaCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the EPA-type in the NE"
    ::= { epaCommon 2 }

-- table of states entry(ro/rw)

    epaCommonEntry OBJECT-TYPE
        SYNTAX EpaCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { epaNESlot }
    ::= { epaCommonTable 1 }

-- the structure of the entry

    EpaCommonEntry ::= SEQUENCE
    {
        epaNESlot                      NESlotValue,
        epaCommonType                  DisplayString,
        epaCommonDescr                 DisplayString,
        epaCommonFirmwareId            DisplayString,
        epaCommonModuleWidth           ModuleWidthValue
    }

    epaNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information.
        Equal to: modEPAv11UniqueID"
    ::= { epaCommonEntry 1 }

    epaCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { epaCommonEntry 2 }

    epaCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { epaCommonEntry 3 }

    epaCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { epaCommonEntry 4 }

    epaCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= { epaCommonEntry 5 }

