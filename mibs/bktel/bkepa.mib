--*****************************************************************************************
--  epaStates group

-- Note: For every control OId there must be a correspondant alarm OId!

    epaStatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EpaStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { epaStates 1 }

-- table of states entry(ro/rw)

    epaStatesEntry OBJECT-TYPE
        SYNTAX EpaStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { epaNESlot }
    ::= { epaStatesTable 1 }

-- the structure of the entry

    EpaStatesEntry ::= SEQUENCE
    {
        epaStatesBootloader          PerceivedSeverityValue,
        epaStatesCommLoss            PerceivedSeverityValue,
        epaStatesInitializing        PerceivedSeverityValue,
        epaStatesSleeping            PerceivedSeverityValue,
        epaStatesTemperatureLow      PerceivedSeverityValue,
        epaStatesTemperatureHigh     PerceivedSeverityValue,
        epaStatesInputLow            PerceivedSeverityValue,
        epaStatesInputHigh           PerceivedSeverityValue,
        epaStatesOutputLow           PerceivedSeverityValue,
        epaStatesOutputHigh          PerceivedSeverityValue,
        epaStatesRfGainReadLow       PerceivedSeverityValue,
        epaStatesRfGainReadHigh      PerceivedSeverityValue,
        epaStatesPlus3p3VLow         PerceivedSeverityValue,
        epaStatesPlus3p3VHigh        PerceivedSeverityValue,
        epaStatesPlus5VLow           PerceivedSeverityValue,
        epaStatesPlus5VHigh          PerceivedSeverityValue,
        epaStatesPlus24VLow          PerceivedSeverityValue,
        epaStatesPlus24VHigh         PerceivedSeverityValue,
        epaStatesSlotDetection       PerceivedSeverityValue,
        epaStatesRfAttenLimit        PerceivedSeverityValue,
        epaStatesObsolete1           PerceivedSeverityValue, -- epaStatesRedundancyActivated
        epaStatesObsolete2           PerceivedSeverityValue, -- epaStatesIsRedundantDevice
        epaStatesRedundancySwitch    PerceivedSeverityValue
    }

    epaStatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The device is running in bootloader mode
             without a legal application software."
    ::= { epaStatesEntry 1 }

    epaStatesCommLoss OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device.
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not
             by the device"
    ::= { epaStatesEntry 2 }

    epaStatesInitializing OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The device is initializing"
    ::= { epaStatesEntry 3 }

    epaStatesSleeping OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device is sleeping, no rf output"
    ::= { epaStatesEntry 4 }

    epaStatesTemperatureLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature low"
    ::= { epaStatesEntry 5 }

    epaStatesTemperatureHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature high"
    ::= { epaStatesEntry 6 }

    epaStatesInputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "rf input (total rms) low"
    ::= { epaStatesEntry 7 }

    epaStatesInputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "rf input (total rms) high"
    ::= { epaStatesEntry 8 }

    epaStatesOutputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "rf output (total rms) low"
    ::= { epaStatesEntry 9 }

    epaStatesOutputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "rf output (total rms) high"
    ::= { epaStatesEntry 10 }

    epaStatesRfGainReadLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Rf gain low"
    ::= { epaStatesEntry 11 }

    epaStatesRfGainReadHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Rf gain high"
    ::= { epaStatesEntry 12 }

    epaStatesPlus3p3VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "3.3 Volt supply low"
    ::= { epaStatesEntry 13 }

    epaStatesPlus3p3VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "3.3 Volt supply high"
    ::= { epaStatesEntry 14 }

    epaStatesPlus5VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "5 Volt supply low"
    ::= { epaStatesEntry 15 }

    epaStatesPlus5VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "5 Volt supply high"
    ::= { epaStatesEntry 16 }

    epaStatesPlus24VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "24 Volt supply low"
    ::= { epaStatesEntry 17 }

    epaStatesPlus24VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "24 Volt supply high"
    ::= { epaStatesEntry 18 }

    epaStatesSlotDetection OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Slot detection not possible"
    ::= { epaStatesEntry 19 }

    epaStatesRfAttenLimit OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "RF attenuator limit reached"
    ::= { epaStatesEntry 20 }

    epaStatesObsolete1 OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Obsolete - Always returns clear(5)"
    ::= { epaStatesEntry 21 }

    epaStatesObsolete2 OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Obsolete - Always returns clear(5)"
    ::= { epaStatesEntry 22 }

    epaStatesRedundancySwitch OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "This (redundant) device has been activated (redundancy switchover)"
    ::= { epaStatesEntry 23 }


--*****************************************************************************************
--  epaControl group

    epaControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EpaControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { epaControl 1 }

-- table of control entry(ro/rw)

    epaControlEntry OBJECT-TYPE
        SYNTAX EpaControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { epaNESlot }
    ::= { epaControlTable 1 }

-- the structure of the entry

    EpaControlEntry ::= SEQUENCE
    {
        epaControlActivityMode     DeviceActivityMode,
        epaControlReset            TruthValue,
        epaControlModuleLedBlink   TruthValue
    }

    epaControlActivityMode OBJECT-TYPE
        SYNTAX DeviceActivityMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION 
            "Device's activity mode.
             Note that elder devices only support the alwaysActive(2) mode,
             a set request to other modes results in a BadValue response here"
    ::= { epaControlEntry 1 }

    epaControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { epaControlEntry 2 }

    epaControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { epaControlEntry 3 }

--*****************************************************************************************
-- epaConfiguration group

    epaConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EpaConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { epaConfiguration 1 }

-- table of configuration entry(ro/rw)

    epaConfigurationEntry OBJECT-TYPE
        SYNTAX EpaConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { epaNESlot  }
    ::= { epaConfigurationTable 1 }

-- the structure of the entry

    EpaConfigurationEntry ::= SEQUENCE
    {
        epaConfigNESlotWrite         NESlotWriteValue,
        epaConfigAgcMode             AgcMode,
        epaConfigRfMode              RfMode,
        epaConfigRfGain              INTEGER,
        epaConfigRfGainLimit         INTEGER,
        epaConfigSlope               INTEGER,
        epaConfigRfOutputAdj         INTEGER,
        epaConfigRedundancyMode      RedundancyMode,

        epaConfigRfInputLimitLoLo    INTEGER,
        epaConfigRfInputLimitLo      INTEGER,
        epaConfigRfInputLimitHi      INTEGER,
        epaConfigRfInputLimitHiHi    INTEGER,

        epaConfigRfGainReadLimitLoLo INTEGER,
        epaConfigRfGainReadLimitLo   INTEGER,
        epaConfigRfGainReadLimitHi   INTEGER,
        epaConfigRfGainReadLimitHiHi INTEGER,

        epaConfigRfOutputLimitLoLo   INTEGER,
        epaConfigRfOutputLimitLo     INTEGER,
        epaConfigRfOutputLimitHi     INTEGER,
        epaConfigRfOutputLimitHiHi   INTEGER
    }

    epaConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
   ::= { epaConfigurationEntry 1 }

    epaConfigAgcMode OBJECT-TYPE
        SYNTAX AgcMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The AGC mode."
    ::= { epaConfigurationEntry 2 }

