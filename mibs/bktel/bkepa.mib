-- Display group

    epaDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF EpaDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { epaDisplay 1 }

-- table of configuration entry(ro/rw)

    epaDisplayEntry OBJECT-TYPE
        SYNTAX EpaDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { epaNESlot  }
    ::= { epaDisplayTable 1 }

-- the structure of the entry

    EpaDisplayEntry ::= SEQUENCE
    {
        epaDisplayRedundancySupported         TruthValue,
        epaDisplayDeviceRedundancyState       DeviceRedundancyState,
        epaDisplayDeviceActivity              DeviceActivityState
    }

    epaDisplayRedundancySupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports redundancy"
    ::={ epaDisplayEntry 1 }

    epaDisplayDeviceRedundancyState OBJECT-TYPE
        SYNTAX  DeviceRedundancyState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the device's redundancy state"
    ::={ epaDisplayEntry 2 }

    epaDisplayDeviceActivity OBJECT-TYPE
        SYNTAX  DeviceActivityState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the actual device's activity state"
    ::={ epaDisplayEntry 3 }



