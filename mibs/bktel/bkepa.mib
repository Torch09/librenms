        epaRfGainReadLo                INTEGER,
        epaRfGainReadValue             INTEGER,
        epaRfGainReadHi                INTEGER,
        epaRfGainReadHiHi              INTEGER,

        epaPlus3p3VLoLo                INTEGER,
