--*****************************************************************************************
-- Display group

    psc24bDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Psc24bDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { psc24bDisplay 1 }

-- table of configuration entry(ro/rw)

    psc24bDisplayEntry OBJECT-TYPE
        SYNTAX Psc24bDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { psc24bNESlot  }
    ::= { psc24bDisplayTable 1 }

-- the structure of the entry

    Psc24bDisplayEntry ::= SEQUENCE
    {
        psc24bDisplayInputVoltageMode   DisplayString,
        psc24bDisplayVoltage2Supported  TruthValue
    }

    psc24bDisplayInputVoltageMode OBJECT-TYPE
        SYNTAX DisplayString (SIZE (2))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Input Voltage Mode, 'AC' or 'DC'"
    ::={ psc24bDisplayEntry 1 }

    psc24bDisplayVoltage2Supported OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Returns true(1) if a secondary voltage is supported by the module."
    ::={ psc24bDisplayEntry 2 }


