BKTEL-HFC862-PSC24B-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    psc24b                 OBJECT IDENTIFIER ::= { modules 111 }
    psc24bCommon           OBJECT IDENTIFIER ::= { psc24b 1 }
    psc24bStates           OBJECT IDENTIFIER ::= { psc24b 2 }
    psc24bConfiguration    OBJECT IDENTIFIER ::= { psc24b 3 }
    psc24bControl          OBJECT IDENTIFIER ::= { psc24b 4 }
    psc24bMeasuringValues  OBJECT IDENTIFIER ::= { psc24b 5 }
    psc24bDisplay          OBJECT IDENTIFIER ::= { psc24b 6 }


--
-- Type definitions
--

    NESlotWriteValue ::= INTEGER (-1..99)



-- psc24bCommon group

    psc24bCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { psc24bCommon 1 }

    psc24bCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Psc24bCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the PSC24B-type in the NE"
    ::= { psc24bCommon 2 }

-- table of states entry(ro/rw)

    psc24bCommonEntry OBJECT-TYPE
        SYNTAX Psc24bCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { psc24bNESlot }
    ::= { psc24bCommonTable 1 }

-- the structure of the entry

    Psc24bCommonEntry ::= SEQUENCE
    {
        psc24bNESlot                      NESlotValue,
        psc24bCommonType                  DisplayString,
        psc24bCommonDescr                 DisplayString,
        psc24bCommonFirmwareId            DisplayString,
        psc24bCommonModuleWidth           ModuleWidthValue
    }

    psc24bNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information.
        Equal to: modOTXDv11UniqueID"
    ::= { psc24bCommonEntry 1 }

    psc24bCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module."
    ::= { psc24bCommonEntry 2 }

    psc24bCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { psc24bCommonEntry 3 }

    psc24bCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { psc24bCommonEntry 4 }

    psc24bCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION
        "The width of the module in multiples of slots (1, 2, ...)
        Returns 0 for special housings (for example 19inch, etc...)"
    ::= { psc24bCommonEntry 5 }


--*****************************************************************************************
--  psc24bStates group

    psc24bStatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Psc24bStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { psc24bStates 1 }

-- table of states entry(ro/rw)

    psc24bStatesEntry OBJECT-TYPE
        SYNTAX Psc24bStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { psc24bNESlot }
    ::= { psc24bStatesTable 1 }

-- the structure of the entry

    Psc24bStatesEntry ::= SEQUENCE
    {
        psc24bStatesBootloader          PerceivedSeverityValue,
        psc24bStatesCommLoss            PerceivedSeverityValue,
        psc24bStatesInputVoltageLow     PerceivedSeverityValue,
        psc24bStatesInputVoltageHigh    PerceivedSeverityValue,
        psc24bStatesOutputVoltageLow    PerceivedSeverityValue,
        psc24bStatesOutputVoltageHigh   PerceivedSeverityValue,
        psc24bStatesOutputCurrentLow    PerceivedSeverityValue,
        psc24bStatesOutputCurrentHigh   PerceivedSeverityValue,
        psc24bStatesOutputVoltage2Low   PerceivedSeverityValue,
        psc24bStatesOutputVoltage2High  PerceivedSeverityValue,
        psc24bStatesOutputCurrent2Low   PerceivedSeverityValue,
        psc24bStatesOutputCurrent2High  PerceivedSeverityValue,
        psc24bStatesPlus3p3VLow         PerceivedSeverityValue,
        psc24bStatesPlus3p3VHigh        PerceivedSeverityValue,
        psc24bStatesPlus5VLow           PerceivedSeverityValue,
        psc24bStatesPlus5VHigh          PerceivedSeverityValue,
        psc24bStatesTemperatureLow      PerceivedSeverityValue,
        psc24bStatesTemperatureHigh     PerceivedSeverityValue
    }

    psc24bStatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The device is running in bootloader mode
             without a legal application software."
    ::= { psc24bStatesEntry 1 }

    psc24bStatesCommLoss OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device.
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not
             by the device"
    ::= { psc24bStatesEntry 2 }

    psc24bStatesInputVoltageLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage low"
    ::= { psc24bStatesEntry 3 }

    psc24bStatesInputVoltageHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage high"
    ::= { psc24bStatesEntry 4 }

    psc24bStatesOutputVoltageLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Main output voltage low"
    ::= { psc24bStatesEntry 5 }

    psc24bStatesOutputVoltageHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Main output voltage high"
    ::= { psc24bStatesEntry 6 }

    psc24bStatesOutputCurrentLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Main output current low"
    ::= { psc24bStatesEntry 7 }

    psc24bStatesOutputCurrentHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Main output current high"
    ::= { psc24bStatesEntry 8 }

    psc24bStatesOutputVoltage2Low OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Secondary output voltage low"
    ::= { psc24bStatesEntry 9 }

    psc24bStatesOutputVoltage2High OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Secondary output voltage high"
    ::= { psc24bStatesEntry 10 }

    psc24bStatesOutputCurrent2Low OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Secondary output current low"
    ::= { psc24bStatesEntry 11 }

    psc24bStatesOutputCurrent2High OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Secondary output current high"
    ::= { psc24bStatesEntry 12 }

    psc24bStatesPlus3p3VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "3.3 Volt internal voltage low"
    ::= { psc24bStatesEntry 13 }

    psc24bStatesPlus3p3VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "3.3 Volt internal voltage high"
    ::= { psc24bStatesEntry 14 }

    psc24bStatesPlus5VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "5 Volt internal voltage low"
    ::= { psc24bStatesEntry 15 }

    psc24bStatesPlus5VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "5 Volt internal voltage high"
    ::= { psc24bStatesEntry 16 }

    psc24bStatesTemperatureLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Internal temperature low"
    ::= { psc24bStatesEntry 17 }

    psc24bStatesTemperatureHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Internal temperature high"
    ::= { psc24bStatesEntry 18 }


--*****************************************************************************************
--  psc24bControl group

    psc24bControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Psc24bControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { psc24bControl 1 }

-- table of control entry(ro/rw)

    psc24bControlEntry OBJECT-TYPE
        SYNTAX Psc24bControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { psc24bNESlot }
    ::= { psc24bControlTable 1 }

-- the structure of the entry

    Psc24bControlEntry ::= SEQUENCE
    {
        psc24bControlReset            TruthValue,
        psc24bControlModuleLedBlink   TruthValue
    }

    psc24bControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { psc24bControlEntry 1 }

    psc24bControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { psc24bControlEntry 2 }


--*****************************************************************************************
-- psc24bConfiguration group

    psc24bConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Psc24bConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { psc24bConfiguration 1 }

-- table of configuration entry(ro/rw)

    psc24bConfigurationEntry OBJECT-TYPE
        SYNTAX Psc24bConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { psc24bNESlot  }
    ::= { psc24bConfigurationTable 1 }

-- the structure of the entry

    Psc24bConfigurationEntry ::= SEQUENCE
    {
        psc24bConfigNESlotWrite                  NESlotWriteValue
    }

    psc24bConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { psc24bConfigurationEntry 1 }


--*****************************************************************************************
-- psc24bMeasuringValues group

-- table of measuring Values

    psc24bMeasuringValuesTable OBJECT-TYPE
