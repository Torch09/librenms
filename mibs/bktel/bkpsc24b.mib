        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { psc24bNESlot }
  ::= { psc24bMeasuringValuesTable 1 }

--  the structure of the entry

    Psc24bMeasuringValuesEntry ::= SEQUENCE
    {
        psc24bInputVoltageLoLo    INTEGER,
        psc24bInputVoltageLo      INTEGER,
        psc24bInputVoltageValue   INTEGER,
        psc24bInputVoltageHi      INTEGER,
        psc24bInputVoltageHiHi    INTEGER,

        psc24bOutputVoltageLoLo   INTEGER,
        psc24bOutputVoltageLo     INTEGER,
        psc24bOutputVoltageValue  INTEGER,
        psc24bOutputVoltageHi     INTEGER,
        psc24bOutputVoltageHiHi   INTEGER,

        psc24bOutputCurrentLoLo   INTEGER,
        psc24bOutputCurrentLo     INTEGER,
        psc24bOutputCurrentValue  INTEGER,
        psc24bOutputCurrentHi     INTEGER,
        psc24bOutputCurrentHiHi   INTEGER,

        psc24bOutputVoltage2LoLo  INTEGER,
        psc24bOutputVoltage2Lo    INTEGER,
        psc24bOutputVoltage2Value INTEGER,
        psc24bOutputVoltage2Hi    INTEGER,
        psc24bOutputVoltage2HiHi  INTEGER,

        psc24bOutputCurrent2LoLo  INTEGER,
        psc24bOutputCurrent2Lo    INTEGER,
        psc24bOutputCurrent2Value INTEGER,
        psc24bOutputCurrent2Hi    INTEGER,
        psc24bOutputCurrent2HiHi  INTEGER,

        psc24bPlus3p3VLoLo        INTEGER,
