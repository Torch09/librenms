        psc24bPlus5VHiHi          INTEGER,

        psc24bTemperatureLoLo     INTEGER,
        psc24bTemperatureLo       INTEGER,
        psc24bTemperatureValue    INTEGER,
        psc24bTemperatureHi       INTEGER,
        psc24bTemperatureHiHi     INTEGER
    }

