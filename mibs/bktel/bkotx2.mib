             Only usable if otx2DisplayPilotPskSignallingSupported == true(1)"
    ::= { otx2ConfigurationEntry 25 }

    otx2ConfigLnbSupplyTx1 OBJECT-TYPE
        SYNTAX LnbSupplyValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "SAT LNB supply control. (Transmitter no. 1)
             not supported for all devces, see otx2DisplayLnbSupplySupported"
    ::= { otx2ConfigurationEntry 26 }

    otx2ConfigLnbSupplyTx2 OBJECT-TYPE
        SYNTAX LnbSupplyValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "SAT LNB supply control. (Transmitter no. 2)
             not supported for all devces, see otx2DisplayLnbSupplySupported"
    ::= { otx2ConfigurationEntry 27 }

    otx2ConfigDeviceRedundancySetting OBJECT-TYPE
        SYNTAX DeviceRedundancySettingValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The device's redundancy modes"
    ::= { otx2ConfigurationEntry 28 }

    otx2ConfigDeviceRedundancyDetection OBJECT-TYPE
        SYNTAX DeviceRedundancyDetectionValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The device's redundancy 'default nominal' or 'default redundant' detect mode,
