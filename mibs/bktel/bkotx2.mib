BKTEL-HFC862-OTX2-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    otx2                  OBJECT IDENTIFIER ::= { modules 117 }
    otx2Common            OBJECT IDENTIFIER ::= { otx2 1 }
    otx2States            OBJECT IDENTIFIER ::= { otx2 2 }
    otx2Configuration     OBJECT IDENTIFIER ::= { otx2 3 }
    otx2Control           OBJECT IDENTIFIER ::= { otx2 4 }
    otx2MeasuringValues   OBJECT IDENTIFIER ::= { otx2 5 }
    otx2Display           OBJECT IDENTIFIER ::= { otx2 6 }


--
-- Type definitions
--

        AgcMode ::= INTEGER
            {
            modeAgcOff(1),
            modeAgcOn(2)
            }

        PilotMode ::= INTEGER
            {
            modePilotOff(1),
            modePilotOn(2)
            }

        PilotPskUsageValue ::= INTEGER
            {
            enableLinecodes(1),
            disableAll(2),
            -- the following mode is only supported if otx2DisplayFullTransbusSupported == true(1)
            enableFullTransbus(3)
            }

        ActivityState ::= INTEGER
            {
            standby(1),
            active(2)
            }

        DeviceRedundancySettingValue ::= INTEGER
            {
            noRedundancy(1),
            onDeviceTransmitterRedundancy(2),
            interDeviceRedundancy(3),
            interDeviceTransmitterRedundancy(4)
            }

        DeviceRedundancyDetectionValue ::= INTEGER
            {
            automaticBySlotNumber(1),
            manualBySetup(2)
            }

        DeviceNominalRedundantValue ::= INTEGER
            {
            nominalDevice(1),
            redundantDevice(2)
            }

        DeviceRedundancyState ::= INTEGER
            {
            nominalDevice(1),
            redundantDevice(2)
            }

        LnbSupplyValue ::= INTEGER
            {
            lnbSupplyOff(1),
            lnbSupply14V(2),
            lnbSupply18V(3),
            lnbSupply14V_22kHz(4),
            lnbSupply18V_22kHz(5)
            }

        LnbSupplySupportedValue ::= INTEGER
            {
            notSupported(1),
            lnbSupply14V18VSwitchSupported(2),
            lnbSupply14V18VAnd22kHzSwitchSupported(3)
            }

        LineCodeNumberValue ::= INTEGER (0..65535)

        NESlotWriteValue ::= INTEGER (-1..99)


