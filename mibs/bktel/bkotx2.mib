--*****************************************************************************************
--  otx2States group

-- Note: For every control OId there must be a correspondant alarm OId!

    otx2StatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Otx2StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otx2States 1 }

-- table of states entry(ro/rw)

    otx2StatesEntry OBJECT-TYPE
        SYNTAX Otx2StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { otx2NESlot }
    ::= { otx2StatesTable 1 }

-- the structure of the entry

    Otx2StatesEntry ::= SEQUENCE
    {
        otx2StatesBootloader                 PerceivedSeverityValue,
        otx2StatesCommLoss                   PerceivedSeverityValue,
        otx2StatesInitializing               PerceivedSeverityValue,
        otx2StatesTransmitterShutdownTx1     PerceivedSeverityValue,
        otx2StatesTransmitterShutdownTx2     PerceivedSeverityValue,
        otx2StatesTemperatureLow             PerceivedSeverityValue,
        otx2StatesTemperatureHigh            PerceivedSeverityValue,
        otx2StatesRfInputTx1Low              PerceivedSeverityValue,
        otx2StatesRfInputTx2Low              PerceivedSeverityValue,
        otx2StatesRfInputTx1High             PerceivedSeverityValue,
        otx2StatesRfInputTx2High             PerceivedSeverityValue,
        otx2StatesOutputPowerTx1             PerceivedSeverityValue,
        otx2StatesOutputPowerTx2             PerceivedSeverityValue,
        otx2StatesOmiReadTx1Low              PerceivedSeverityValue,
        otx2StatesOmiReadTx2Low              PerceivedSeverityValue,
        otx2StatesOmiReadTx1High             PerceivedSeverityValue,
        otx2StatesOmiReadTx2High             PerceivedSeverityValue,
        otx2StatesRfGainReadTx1Low           PerceivedSeverityValue,
        otx2StatesRfGainReadTx2Low           PerceivedSeverityValue,
        otx2StatesRfGainReadTx1High          PerceivedSeverityValue,
        otx2StatesRfGainReadTx2High          PerceivedSeverityValue,
        otx2StatesLaserCurrentTx1            PerceivedSeverityValue,
        otx2StatesLaserCurrentTx2            PerceivedSeverityValue,
        otx2StatesTecCurrentTx1              PerceivedSeverityValue,
        otx2StatesTecCurrentTx2              PerceivedSeverityValue,
        otx2StatesLaserTempOffsetTx1         PerceivedSeverityValue,
        otx2StatesLaserTempOffsetTx2         PerceivedSeverityValue,
        otx2StatesRfRegulationLimitTx1       PerceivedSeverityValue,
        otx2StatesRfRegulationLimitTx2       PerceivedSeverityValue,
        otx2StatesPilotRegulationLimitTx1    PerceivedSeverityValue,
        otx2StatesPilotRegulationLimitTx2    PerceivedSeverityValue,
        otx2StatesRedundancySwitchTx1        PerceivedSeverityValue,
        otx2StatesRedundancySwitchTx2        PerceivedSeverityValue,
        otx2StatesInputVoltageLow            PerceivedSeverityValue,
        otx2StatesInputVoltageHigh           PerceivedSeverityValue,
        otx2StatesInternalVoltage            PerceivedSeverityValue,
        otx2StatesPowerSupplyLeft            PerceivedSeverityValue,
        otx2StatesPowerSupplyRight           PerceivedSeverityValue,
        otx2StatesFanLeft                    PerceivedSeverityValue,
        otx2StatesFanRight                   PerceivedSeverityValue
    }

    otx2StatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The device is running in bootloader mode
             without a legal application software."
    ::= { otx2StatesEntry 1 }

    otx2StatesCommLoss OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device.
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not
             by the device"
    ::= { otx2StatesEntry 2 }

