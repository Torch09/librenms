    ::= { otx2ConfigurationEntry 20 }

    otx2ConfigPilotModeTx2 OBJECT-TYPE
        SYNTAX PilotMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Pilot On/Off. (Transmitter no. 2)"
    ::= { otx2ConfigurationEntry 21 }

