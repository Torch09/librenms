        SYNTAX PilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Pilot PSK coding usage. (Transmitter no. 1)"
    ::= { otx2ConfigurationEntry 22 }

    otx2ConfigPilotPskUsageTx2 OBJECT-TYPE
        SYNTAX PilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Pilot PSK coding usage. (Transmitter no. 2)"
    ::= { otx2ConfigurationEntry 23 }

    otx2ConfigLineCodeNumberTx1 OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The line code number that the transmitter shall send. (Transmitter no. 1)
