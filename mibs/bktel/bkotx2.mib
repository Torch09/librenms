    otx2ConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
   ::= { otx2ConfigurationEntry 1 }

    otx2ConfigAgcModeTx1 OBJECT-TYPE
        SYNTAX AgcMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The AGC mode. (Transmitter no. 1)"
    ::= { otx2ConfigurationEntry 2 }

    otx2ConfigAgcModeTx2 OBJECT-TYPE
        SYNTAX AgcMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The AGC mode. (Transmitter no. 2)"
    ::= { otx2ConfigurationEntry 3 }

    otx2ConfigOmiTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal optical modulation index in 0.1 dB units. (Transmitter no. 1)
            This parameter is only used if AGC is on."
    ::= { otx2ConfigurationEntry 4 }

    otx2ConfigOmiTx2 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal optical modulation index in 0.1 dB units. (Transmitter no. 2)
                     This parameter is only used if AGC is on."
    ::= { otx2ConfigurationEntry 5 }

    otx2ConfigRfGainTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired RF gain in 0.1 dB units. (Transmitter no. 1)
            This parameter is only used if AGC is off."
    ::= { otx2ConfigurationEntry 6 }

    otx2ConfigRfGainTx2 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired RF gain in 0.1 dB units. (Transmitter no. 2)
            This parameter is only used if AGC is off."
    ::= { otx2ConfigurationEntry 7 }

    otx2ConfigRfGainLimitTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The maximum RF gain in 0.1 dB units. (Transmitter no. 1)
            This parameter is only used if AGC is ON."
    ::= { otx2ConfigurationEntry 8 }

    otx2ConfigRfGainLimitTx2 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The maximum RF gain in 0.1 dB units. (Transmitter no. 2)
            This parameter is only used if AGC is ON."
    ::= { otx2ConfigurationEntry 9 }

    otx2ConfigSlopeTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired slope in 0.1 dB units. (Transmitter no. 1)"
    ::= { otx2ConfigurationEntry 10 }

    otx2ConfigSlopeTx2 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired slope in 0.1 dB units. (Transmitter no. 2)"
    ::= { otx2ConfigurationEntry 11 }

    otx2ConfigFiberLengthTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The optical fiber length in meter units. (Transmitter no. 1)
                     only usable if otx2DisplayChirpCompensationSupported"
    ::= { otx2ConfigurationEntry 12 }

    otx2ConfigFiberLengthTx2 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The optical fiber length in meter units. (Transmitter no. 2)
                     only usable if otx2DisplayChirpCompensationSupported"
    ::= { otx2ConfigurationEntry 13 }

    otx2ConfigPilotOmiTx1 OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The pilot omi in 0.1 dB units. (Transmitter no. 1)
