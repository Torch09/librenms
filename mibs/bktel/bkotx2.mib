-- MibCompiler defs --


--*****************************************************************************************
-- OTX2 ( Dual direct modulated optical transmitter )
--*****************************************************************************************

-- otx2Common group

    otx2CommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { otx2Common 1 }

    otx2CommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Otx2CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OTX2-type in the NE"
    ::= { otx2Common 2 }

-- table of states entry(ro/rw)

    otx2CommonEntry OBJECT-TYPE
        SYNTAX Otx2CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { otx2NESlot }
    ::= { otx2CommonTable 1 }

-- the structure of the entry

    Otx2CommonEntry ::= SEQUENCE
    {
        otx2NESlot                      NESlotValue,
        otx2CommonType                  DisplayString,
        otx2CommonDescr                 DisplayString,
        otx2CommonFirmwareId            DisplayString,
        otx2CommonModuleWidth           ModuleWidthValue
    }

    otx2NESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { otx2CommonEntry 1 }

    otx2CommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { otx2CommonEntry 2 }

    otx2CommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { otx2CommonEntry 3 }

    otx2CommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { otx2CommonEntry 4 }

    otx2CommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION
        "The width of the module in multiples of slots (1, 2, ...)
        Returns 0 for special housings (for example 19inch, etc...)"
    ::= { otx2CommonEntry 5 }

