--*****************************************************************************************
--  otx2Control group

    otx2ControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Otx2ControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otx2Control 1 }

-- table of control entry(ro/rw)

    otx2ControlEntry OBJECT-TYPE
        SYNTAX Otx2ControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { otx2NESlot }
    ::= { otx2ControlTable 1 }

-- the structure of the entry

    Otx2ControlEntry ::= SEQUENCE
    {
        otx2ControlLaserShutdownTx1 TruthValue,
        otx2ControlLaserShutdownTx2 TruthValue,
        otx2ControlReset            TruthValue,
        otx2ControlModuleLedBlink   TruthValue
    }

    otx2ControlLaserShutdownTx1 OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Force transmitter no. 1 into standy/shutdown mode"
    ::= { otx2ControlEntry 1 }

    otx2ControlLaserShutdownTx2 OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Force transmitter no. 2 into standy/shutdown mode"
    ::= { otx2ControlEntry 2 }

    otx2ControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { otx2ControlEntry 3 }

    otx2ControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { otx2ControlEntry 4 }

