    {
        otxdControlLaserShutdown    TruthValue,
        otxdControlReset            TruthValue,
        otxdControlModuleLedBlink   TruthValue
    }

    otxdControlLaserShutdown OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "This parameter is obsolete, always returns 'false(2)' !!!
             Use 'otxdConfigRedundancyMode' to set the device's redundancy issues.
             Use 'otxdDisplayDeviceActivity' to read the device's actual activity."
    ::= { otxdControlEntry 1 }

    otxdControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { otxdControlEntry 2 }

    otxdControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { otxdControlEntry 3 }

--*****************************************************************************************
-- otxdConfiguration group

    otxdConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtxdConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otxdConfiguration 1 }

-- table of configuration entry(ro/rw)

    otxdConfigurationEntry OBJECT-TYPE
        SYNTAX OtxdConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { otxdNESlot  }
    ::= { otxdConfigurationTable 1 }

-- the structure of the entry

    OtxdConfigurationEntry ::= SEQUENCE
    {
        otxdConfigNESlotWrite         NESlotWriteValue,
        otxdConfigAgcMode             AgcMode,
        otxdConfigRfMode              RfMode,
        otxdConfigOmi                 INTEGER,
        otxdConfigRfGain              INTEGER,
        otxdConfigRfGainLimit         INTEGER,
        otxdConfigSlope               INTEGER,
        otxdConfigFiberLength         INTEGER,
        otxdConfigPilotFrequency      PilotFrequency,
        otxdConfigPilotOmi            INTEGER,
        otxdConfigLaserFrequency      INTEGER,
        otxdConfigOutputPowerAdj      INTEGER,
        otxdConfigPilotPskUsage       PilotPskUsageValue,
        otxdConfigLineCodeNumber      LineCodeNumberValue,

        otxdConfigRfInputLimitLoLo    INTEGER,
        otxdConfigRfInputLimitLo      INTEGER,
        otxdConfigRfInputLimitHi      INTEGER,
        otxdConfigRfInputLimitHiHi    INTEGER,

        otxdConfigRfGainReadLimitLoLo INTEGER,
        otxdConfigRfGainReadLimitLo   INTEGER,
        otxdConfigRfGainReadLimitHi   INTEGER,
        otxdConfigRfGainReadLimitHiHi INTEGER,

        otxdConfigOmiReadLimitLoLo    INTEGER,
        otxdConfigOmiReadLimitLo      INTEGER,
        otxdConfigOmiReadLimitHi      INTEGER,
        otxdConfigOmiReadLimitHiHi    INTEGER,

        otxdConfigOutputPwrLimitLoLo  INTEGER,
        otxdConfigOutputPwrLimitLo    INTEGER,
        otxdConfigOutputPwrLimitHi    INTEGER,
        otxdConfigOutputPwrLimitHiHi  INTEGER,

        otxdConfigWavelenOfsLimitLoLo INTEGER,
        otxdConfigWavelenOfsLimitLo   INTEGER,
        otxdConfigWavelenOfsLimitHi   INTEGER,
        otxdConfigWavelenOfsLimitHiHi INTEGER,

        otxdConfigPilotMode           PilotMode,
        otxdConfigNarrowCastGain      INTEGER,
        otxdConfigPilot2Omi           INTEGER,
        otxdConfigPilot2Mode          PilotMode,

        otxdConfigRedundancyMode      RedundancyMode,

        otxdConfigSatAgcMode          AgcMode,
        otxdConfigSatOmi              INTEGER,
        otxdConfigSatRfGain           INTEGER,
        otxdConfigSatRfGainLimit      INTEGER,
        otxdConfigSatSlope            INTEGER,
        otxdConfigSatLnbSupply        LnbSupplyValue,

        otxdConfigRedundancyMask      RedundancyMask,
        otxdConfigRfInputAlarmMode    RfInputAlarmMode,

        otxdConfigClippingMitigation  INTEGER

    }

    otxdConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
   ::= { otxdConfigurationEntry 1 }

    otxdConfigAgcMode OBJECT-TYPE
        SYNTAX AgcMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The AGC mode."
    ::= { otxdConfigurationEntry 2 }

    otxdConfigRfMode OBJECT-TYPE
        SYNTAX RfMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The RF mode."
    ::= { otxdConfigurationEntry 3 }

    otxdConfigOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal optical modulation index in 0.1 dB units.
            This parameter is only used if AGC is on."
    ::= { otxdConfigurationEntry 4 }

    otxdConfigRfGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal gain in 0.1 dB units.
            This parameter is only used if AGC is off."
    ::= { otxdConfigurationEntry 5 }

    otxdConfigRfGainLimit OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The maximum offset from nominal gain in 0.1 dB units.
            This parameter is only used if AGC is off."
    ::= { otxdConfigurationEntry 6 }

    otxdConfigSlope OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired slope in 0.1 dB units."
    ::= { otxdConfigurationEntry 7 }

    otxdConfigFiberLength OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The optical fiber length in meter units."
    ::= { otxdConfigurationEntry 8 }

    otxdConfigPilotFrequency OBJECT-TYPE
        SYNTAX PilotFrequency
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The pilot frequency."
    ::= { otxdConfigurationEntry 9 }

    otxdConfigPilotOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The pilot omi in 0.1 dB units. (Relative to nomimal value of 5%)"
    ::= { otxdConfigurationEntry 10 }

    otxdConfigLaserFrequency OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The laser frequency offset from nominal freq. in 0.01 GHz units."
    ::= { otxdConfigurationEntry 11 }

    otxdConfigOutputPowerAdj OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired optical output power offset from nominal power in 0.1 dB units."
    ::= { otxdConfigurationEntry 12 }

    otxdConfigPilotPskUsage OBJECT-TYPE
        SYNTAX PilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "PSK coding via pilot usage
             Only usable if otxdDisplayPilotPskSignallingSupported == true(1), otherwise always reports disableAll(2)"
    ::= { otxdConfigurationEntry 13 }

    otxdConfigLineCodeNumber OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The line code number that the transmitter shall send.
             This line code number can be used on the remote side
             optical receiver to identify the connected transmitter.
             Only usable if otxdDisplayPilotPskSignallingSupported == true(1), otherwise always reports zero"
    ::= { otxdConfigurationEntry 14 }

-- *****
    otxdConfigRfInputLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf input power low alarm limit in 0.1 dBm units"
    ::={ otxdConfigurationEntry 15 }

    otxdConfigRfInputLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf input power low warning limit in 0.1 dBm units"
    ::={ otxdConfigurationEntry 16 }

    otxdConfigRfInputLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf input power high warning limit in 0.1 dBm units"
    ::={ otxdConfigurationEntry 17 }

    otxdConfigRfInputLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf input power high alarm limit in 0.1 dBm units"
    ::={ otxdConfigurationEntry 18 }

-- *****
    otxdConfigRfGainReadLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf gain offset low alarm limit in 0.1 dB units (in AGC ON mode)"
    ::={ otxdConfigurationEntry 19 }

    otxdConfigRfGainReadLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf gain offset low warning limit in 0.1 dB units (in AGC ON mode)"
    ::={ otxdConfigurationEntry 20 }

    otxdConfigRfGainReadLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf gain offset high warning limit in 0.1 dB units (in AGC ON mode)"
    ::={ otxdConfigurationEntry 21 }

    otxdConfigRfGainReadLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The rf gain offset high alarm limit in 0.1 dB units (in AGC ON mode)"
    ::={ otxdConfigurationEntry 22 }

-- *****
    otxdConfigOmiReadLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The omi offset low alarm limit in 0.1 dB units (in AGC OFF mode)"
    ::={ otxdConfigurationEntry 23 }

    otxdConfigOmiReadLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The omi offset low warning limit in 0.1 dB units(in AGC OFF mode)"
    ::={ otxdConfigurationEntry 24 }

    otxdConfigOmiReadLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The omi offset high warning limit in 0.1 dB units(in AGC OFF mode)"
    ::={ otxdConfigurationEntry 25 }

    otxdConfigOmiReadLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The omi offset high alarm limit in 0.1 dB units(in AGC OFF mode)"
    ::={ otxdConfigurationEntry 26 }

-- *****
    otxdConfigOutputPwrLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power offset low alarm limit in 0.1 dB units"
    ::={ otxdConfigurationEntry 27 }

    otxdConfigOutputPwrLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power offset low warning limit in 0.1 dB units"
    ::={ otxdConfigurationEntry 28 }

    otxdConfigOutputPwrLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power offset high warning limit in 0.1 dB units"
    ::={ otxdConfigurationEntry 29 }

    otxdConfigOutputPwrLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power offset high alarm limit in 0.1 dB units"
    ::={ otxdConfigurationEntry 30 }

-- *****
    otxdConfigWavelenOfsLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The wavelength offset low alarm limit in 0.01 nm units"
    ::={ otxdConfigurationEntry 31 }

    otxdConfigWavelenOfsLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The wavelength offset low warning limit in 0.01 nm units"
    ::={ otxdConfigurationEntry 32 }

    otxdConfigWavelenOfsLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The wavelength offset high warning limit in 0.01 nm units"
    ::={ otxdConfigurationEntry 33 }

    otxdConfigWavelenOfsLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The wavelength offset high alarm limit in 0.01 nm units"
    ::={ otxdConfigurationEntry 34 }

-- *****
    otxdConfigPilotMode OBJECT-TYPE
        SYNTAX PilotMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Pilot On/Off."
    ::= { otxdConfigurationEntry 35 }

-- *****
    otxdConfigNarrowCastGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired gain for narrowcast input relative to nominal value in 0.1 dB units.
            Only usable if otxdDisplayNarrowCastGainSupported == true(1), otherwise always reports zero"
    ::= { otxdConfigurationEntry 36 }

-- *****
    otxdConfigPilot2Omi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The secondary pilot's omi in 0.1 dB units. (Relative to nomimal value of 5%)
            Only usable if otxdDisplayPilot2Supported == true(1), otherwise always reports zero"
    ::= { otxdConfigurationEntry 37 }

-- *****
    otxdConfigPilot2Mode OBJECT-TYPE
        SYNTAX PilotMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Secondary pilot On/Off.
            Only usable if otxdDisplayPilot2Supported == true(1), otherwise always reports modePilotOff(1)"
    ::= { otxdConfigurationEntry 38 }

-- *****
    otxdConfigRedundancyMode OBJECT-TYPE
        SYNTAX RedundancyMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The device redundancy settings.
            If otxdDisplayRedundancySupported == false(2), only the modes modeAlwaysActive(1) and modeForceStandby(2) are possible.
            If otxdDisplayRedundancySupported == true(1), the modes modeNominalMaster(3), modeRedundantSlave(4) and
            modeNominalRedundantAutomaticBySlot(5) switch device's redundancy ON and adjust its redundancy behaviour."
    ::= { otxdConfigurationEntry 39 }

-- *****
    otxdConfigSatAgcMode OBJECT-TYPE
        SYNTAX AgcMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The SAT AGC mode.
            This parameter is not supported by all transmitter devices,
            see otxdDisplaySatExtendedFeaturesSupported"
    ::= { otxdConfigurationEntry 40 }

    otxdConfigSatOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal SAT optical modulation index in 0.1 dB units.
            This parameter is only used if SAT AGC is on.
            This parameter is not supported by all transmitter devices,
            see otxdDisplaySatExtendedFeaturesSupported"
    ::= { otxdConfigurationEntry 41 }

    otxdConfigSatRfGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal SAT gain in 0.1 dB units.
            This parameter is only used if SAT AGC is off.
            This parameter is not supported by all transmitter devices,
            see otxdDisplaySatExtendedFeaturesSupported"
    ::= { otxdConfigurationEntry 42 }

    otxdConfigSatRfGainLimit OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The maximum offset from nominal SAT gain in 0.1 dB units.
            This parameter is only used if SAT AGC is off.
            This parameter is not supported by all transmitter devices,
            see otxdDisplaySatExtendedFeaturesSupported"
    ::= { otxdConfigurationEntry 43 }

    otxdConfigSatSlope OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired SAT slope in 0.1 dB units.
            This parameter is not supported by all transmitter devices,
            see otxdDisplaySatSlopeSupported"
    ::= { otxdConfigurationEntry 44 }

    otxdConfigSatLnbSupply OBJECT-TYPE
        SYNTAX LnbSupplyValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "SAT LNB supply control.
            This feature is not supported by all transmitter devices,
            see otxdDisplaySatLnbSupplySupported"
    ::= { otxdConfigurationEntry 45 }

    otxdConfigRedundancyMask OBJECT-TYPE
        SYNTAX RedundancyMask
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "The device's redundancy mask setings."
    ::= { otxdConfigurationEntry 46 }

    otxdConfigRfInputAlarmMode OBJECT-TYPE
        SYNTAX RfInputAlarmMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
          "The RF input alarm mode (Can be used to disable alarms of unused RF inputs)
           Only usable if ovtxDisplayRfInputCapabilities == rfInputCatvAndSatSupported(3), 
           otherwise always reports alarmEnableAll(1)"
    ::= { otxdConfigurationEntry 47 }

    otxdConfigClippingMitigation OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Clipping Mitigation. adjustable from 0 .. 20000, step 500"
    ::= { otxdConfigurationEntry 48 }

--*****************************************************************************************
-- Display group

    otxdDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtxdDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otxdDisplay 1 }

-- table of configuration entry(ro/rw)

    otxdDisplayEntry OBJECT-TYPE
        SYNTAX OtxdDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { otxdNESlot  }
    ::= { otxdDisplayTable 1 }

-- the structure of the entry

    OtxdDisplayEntry ::= SEQUENCE
    {
        otxdDisplayLaserFreqNominal             INTEGER,
        otxdDisplayLaserFreqMin                 INTEGER,
        otxdDisplayLaserFreqMax                 INTEGER,
        otxdDisplayLaserFreqStep                INTEGER,

        otxdDisplayOutputPwrNominal             INTEGER,
        otxdDisplayOutputPwrMin                 INTEGER,
        otxdDisplayOutputPwrMax                 INTEGER,
        otxdDisplayOutputPwrStep                INTEGER,

        otxdDisplayOmiNominal                   INTEGER,
        otxdDisplayPilot2Frequency              INTEGER,
        otxdDisplaySatRfInputSupported          TruthValue,
        otxdDisplayNarrowCastGainSupported      TruthValue,
        otxdDisplayPilot2Supported              TruthValue,
        otxdDisplayPilotPskSignallingSupported  TruthValue,
        otxdDisplayFullTransbusSupported        TruthValue,

        otxdDisplayRedundancySupported          TruthValue,
        otxdDisplayDeviceRedundancyState        DeviceRedundancyState,
        otxdDisplayDeviceActivity               DeviceActivityState,

        otxdDisplaySatExtendedFeaturesSupported TruthValue,
        otxdDisplaySatSlopeSupported            TruthValue,
        otxdDisplaySatLnbSupplySupported        LnbSupplySupportedValue,

        otxdDisplayPilotAutomaticModeSupported  TruthValue
    }

    otxdDisplayLaserFreqNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal laser frequency in 0.01 GHz units."
    ::={ otxdDisplayEntry 1 }

    otxdDisplayLaserFreqMin OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The minimum adjustable offset from nominal frequency in 0.01 GHz units."
    ::={ otxdDisplayEntry 2 }

    otxdDisplayLaserFreqMax OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The maximum adjustable offset from nominal frequency in 0.01 GHz units."
    ::={ otxdDisplayEntry 3 }

    otxdDisplayLaserFreqStep OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The supported laser frequency stepsize in 0.01 GHz units."
    ::={ otxdDisplayEntry 4 }

-- *****
    otxdDisplayOutputPwrNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal optical output power in 0.1 dBm units."
    ::={ otxdDisplayEntry 5 }

    otxdDisplayOutputPwrMin OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The minimum adjustable offset from nominal output power in 0.1 dB units."
    ::={ otxdDisplayEntry 6 }

    otxdDisplayOutputPwrMax OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The maximum adjustable offset from nominal output power in 0.1 dB units."
    ::={ otxdDisplayEntry 7 }

    otxdDisplayOutputPwrStep OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The supported output power stepsize in 0.1 dB units."
    ::={ otxdDisplayEntry 8 }

-- *****
    otxdDisplayOmiNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The nominal OMI in 0.1 percent units."
    ::={ otxdDisplayEntry 9 }

-- *****
    otxdDisplayPilot2Frequency OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "The secondary pilot frequency in kHz units.
           Returns 0 if the secondary pilot is not supported"
    ::={ otxdDisplayEntry 10 }

-- *****
    otxdDisplaySatRfInputSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a separate sat rf input signal is supported by device"
    ::={ otxdDisplayEntry 11 }

    otxdDisplayNarrowCastGainSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a narrow cast signalling is supported by device"
    ::={ otxdDisplayEntry 12 }

    otxdDisplayPilot2Supported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if the second pilot signal is supported by device"
    ::={ otxdDisplayEntry 13 }

    otxdDisplayPilotPskSignallingSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if the psk pilot signalling is supported by device"
    ::={ otxdDisplayEntry 14 }

    otxdDisplayFullTransbusSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the vendor specific 'full transbus' is supported by device
            (!! Only for future use !!)"
    ::={ otxdDisplayEntry 15 }

-- *****

    otxdDisplayRedundancySupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports redundancy issues"
    ::={ otxdDisplayEntry 16 }

    otxdDisplayDeviceRedundancyState OBJECT-TYPE
        SYNTAX  DeviceRedundancyState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the device's redundancy state"
    ::={ otxdDisplayEntry 17 }

    otxdDisplayDeviceActivity OBJECT-TYPE
        SYNTAX  DeviceActivityState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the actual device's activity state"
    ::={ otxdDisplayEntry 18 }

-- *****

    otxdDisplaySatExtendedFeaturesSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports the extended
            sat features (Sat AGC, Sat OMI, Sat RF Gain)"
    ::={ otxdDisplayEntry 19 }

    otxdDisplaySatSlopeSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports a sat slope setup"
    ::={ otxdDisplayEntry 20 }

    otxdDisplaySatLnbSupplySupported OBJECT-TYPE
        SYNTAX  LnbSupplySupportedValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the supported LNB supply features"
    ::={ otxdDisplayEntry 21 }

-- *****

    otxdDisplayPilotAutomaticModeSupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports the pilot automatic mode.
            If the pilot automatic mode ist adjusted, the pilot signal gets automatically switched on, 
            if an RF input signal loss condition is detected. Useful for sbs suppression."
    ::={ otxdDisplayEntry 22 }

END



