BKTEL-HFC862-OTXD-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    otxd                  OBJECT IDENTIFIER ::= { modules 109 }
    otxdCommon            OBJECT IDENTIFIER ::= { otxd 1 }
    otxdStates            OBJECT IDENTIFIER ::= { otxd 2 }
    otxdConfiguration     OBJECT IDENTIFIER ::= { otxd 3 }
    otxdControl           OBJECT IDENTIFIER ::= { otxd 4 }
    otxdMeasuringValues   OBJECT IDENTIFIER ::= { otxd 5 }
    otxdDisplay           OBJECT IDENTIFIER ::= { otxd 6 }


--
-- Type definitions
--

        AgcMode ::= INTEGER
            {
            modeAgcOff(1),
            modeAgcOn(2)
            }

        PilotMode ::= INTEGER
            {
            modePilotOff(1),
            modePilotOn(2),
            -- the following mode is only supported if otxdDisplayPilotAutomaticModeSupported == true(1)
            modePilotAutomatic(3) 
            }

        RfMode ::= INTEGER
            {
            modeNormal(1),
            modeLowNoise(2),
            modeLowDistortion(3)
            }

        RfInputAlarmMode ::= INTEGER
            {
            alarmEnableAll(1),
            alarmCatvEnableSatDisable(2),
            alarmSatEnableCatvDisable(3)
            }

        PilotFrequency ::= INTEGER
            {
            -- Default set of pilot frequency data
            frequency_307_kHz(1),
            frequency_614_kHz(2),
            frequency_1229_kHz(3),
            frequency_2458_kHz(4),

            freq_unused5(5),
            freq_unused6(6),
            freq_unused7(7),
            freq_unused8(8),
            freq_unused9(9),
            freq_unused10(10),
            freq_unused11(11),
            freq_unused12(12),
            freq_unused13(13),
            freq_unused14(14),
            freq_unused15(15),
            freq_unused16(16),

            -- Alternate set of pilot frequency data
            frequency_632_kHz(17),
            frequency_3584_kHz(18)
            }

        PilotPskUsageValue ::= INTEGER
            {
            enableLinecodes(1),
            disableAll(2),
            -- the following mode is only supported if otxdDisplayFullTransbusSupported == true(1)
            enableFullTransbus(3)
            }

        RedundancyMode ::= INTEGER
            {
            modeForceStandby(1),
            modeAlwaysActive(2),
            -- the following modes are only supported if otxdDisplayRedundancySupported == true(1)
            modeNominalMaster(3),
            modeRedundantSlave(4),
            modeNominalRedundantAutomaticBySlot(5)
            }

        RedundancyMask ::= INTEGER
            {
            maskAlarmsOnly(1),
            maskAlarmsAndWarnings(2),
            maskSpecialAdjusted(3)           -- This item is NOT writable !!
            }

        DeviceRedundancyState ::= INTEGER
            {
            stateNominalMaster(1),
            stateRedundantSlave(2)
            }

        DeviceActivityState ::= INTEGER
            {
            standby(1),
            active(2)
            }

        LnbSupplyValue ::= INTEGER
            {
            lnbSupplyOff(1),
            lnbSupply14V(2),
            lnbSupply18V(3),
            lnbSupply14V_22kHz(4),
            lnbSupply18V_22kHz(5)
            }

        LnbSupplySupportedValue ::= INTEGER
            {
            notSupported(1),
            lnbSupply14V18VSwitchSupported(2),
            lnbSupply14V18VAnd22kHzSwitchSupported(3)
            }

        LineCodeNumberValue ::= INTEGER (0..65535)

        NESlotWriteValue ::= INTEGER (-1..99)


--*****************************************************************************************
-- OTXD ( Direct modulated optical transmitter )
--*****************************************************************************************

-- otxdCommon group

    otxdCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
    ::= { otxdCommon 1 }

    otxdCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtxdCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OTXD-type in the NE"
    ::= { otxdCommon 2 }

-- table of states entry(ro/rw)

    otxdCommonEntry OBJECT-TYPE
        SYNTAX OtxdCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { otxdNESlot }
    ::= { otxdCommonTable 1 }

-- the structure of the entry

    OtxdCommonEntry ::= SEQUENCE
    {
        otxdNESlot                      NESlotValue,
        otxdCommonType                  DisplayString,
        otxdCommonDescr                 DisplayString,
        otxdCommonFirmwareId            DisplayString,
        otxdCommonModuleWidth           ModuleWidthValue
    }

    otxdNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { otxdCommonEntry 1 }

    otxdCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { otxdCommonEntry 2 }

    otxdCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { otxdCommonEntry 3 }

    otxdCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { otxdCommonEntry 4 }

    otxdCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION
        "The width of the module in multiples of slots (1, 2, ...)
        Returns 0 for special housings (for example 19inch, etc...)"
    ::= { otxdCommonEntry 5 }

