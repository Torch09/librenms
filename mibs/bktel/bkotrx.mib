
-- *********************************************************************************************
-- *
-- * Description:   Implements definitions for the Otrx-modul
-- *                Optical Transceiver/Receiver with one receiver and max. two transmitters
-- *
-- *                NOTE that this file also represents the definition for the new BOR900 receiver
-- *                In this case there are zero transmitters assembled: 
-- *                        otrxDisplayTx1ActivityState == notAssembled(3)
-- *                     and
-- *                        otrxDisplayTx2ActivityState == notAssembled(3)
-- *
-- * Copyright 2009 by BKtel communications GmbH
-- *
-- *     V.0.1 02.06.2006  M. Heldmann first draft !!!
-- *     V.0.2 15.06.2006  several changes
-- *     V.0.3 04.10.2006  M. Heldmann    NESlotWriteValue expanded due to new 2G6 platform needs
-- *     V.0.4 06.11.2006  M. Heldmann    renamed otrxPlus10Vxxxx to otrxPlus15Vxxxx
-- *                       M. Heldmann    renamed TxIngressMode values
-- *                       M. Heldmann    added otrxStatesTx1RegulatorLimit..otrxStatesTx2RegulatorLimit
-- *     V.1.0 28.01.2009  M. Heldmann    * new tranceiver type add-ons:
-- *                                      corrected name RxRfOutputPowerRms to RxRfInputPowerRms
-- *                                      renamed LineCodeUsage to TxPilotPskUsage
-- *                                      renamed voltages in otrxStates and otrxMeasure, added nominal values
-- *                                      expanded RxRegulationMode with pilotBasedRegulation(3)
-- *                                      added states otrxStatesBkRackExtPowerSupply, otrxStatesRxPskRemoteState and otrxStatesRxPilotSignal
-- *                                      added configuration parameters 
-- *                                         otrxConfigBkRackExtPowerSupplyAlarmMode .. otrxConfigRxExpectedLineCodeNumber
-- *                                      added display parameters
-- *                                         otrxDisplayRxPilotBasedRegulationSupported .. otrxDisplayInternalVoltage4Nominal
-- *     V.1.1 11.07.2013  M. Heldmann    adjusted syntax of "CommonEntry" to remove MIB compiler warnings
-- *     V.1.2 05.09.2014  M. Heldmann    changed CommonModuleWidth and ConfigNESlotWrite STATUS to optional
-- *********************************************************************************************

BKTEL-HFC862-OTRX-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    otrx                  OBJECT IDENTIFIER ::= { modules 115 }
    otrxCommon            OBJECT IDENTIFIER ::= { otrx 1 }
    otrxStates            OBJECT IDENTIFIER ::= { otrx 2 }
    otrxConfiguration     OBJECT IDENTIFIER ::= { otrx 3 }
    otrxControl           OBJECT IDENTIFIER ::= { otrx 4 }
    otrxMeasuringValues   OBJECT IDENTIFIER ::= { otrx 5 }
    otrxDisplay           OBJECT IDENTIFIER ::= { otrx 6 }


--
-- Type definitions
--

    BkRackLidNotifyMode ::= INTEGER
        {
        noNotify(1),
        notifyIfBkRackOpen(2)
        }

    BkRackExtPowerSupplyAlarmMode ::= INTEGER
        {
        notSupported(1),
        noAlarmOrWarning(2),
        warningOnPowerSupplyFailure(3),
        alarmOnPowerSupplyFailure(4)
        }

    RedundancySettingValue ::= INTEGER
        {
        noRedundancy(1),
        controlledBySuccedingAmplifierDevice(2),
        controlledByBkRackHardware(3)
        }

    NominalRedundantMode ::= INTEGER
        {
        automaticBySlotNumber(1),
        nominalDevice(2),
        redundantDevice(3)
        }

    NominalRedundantValue ::= INTEGER
        {
        nominalDevice(1),
        redundantDevice(2)
        }

    TxPilotActivityValue ::= INTEGER
        {
        pilotOff(1),
        pilotOn(2)
        }

    TxActivityMode ::= INTEGER
        {
        alwaysActive(1),
        shutdown(2),
        automatic(3)
        }

    TxIngressMode ::= INTEGER
        {
        noAttenuator(1),
        attenuator_6dB(2),
        attenuator_40dB(3)
        }

    TxActivityValue ::= INTEGER
        {
        active(1),
        shutdown(2),
        notAssembled(3)
        }

    RxRegulationMode ::= INTEGER
        {
        noRegulationManualSetup(1),
        inputPowerBasedRegulation(2),
        pilotBasedRegulation(3)  -- not supported by all devices, see otrxDisplayRxPilotBasedRegulationSupported
        }

    RfOutputLevelRangeMode ::= INTEGER
        {
        defaultUsingDeviceSpecs(1),
        extendedInputPowerBased(2)
        }

    RxInputWavelengthValue ::= INTEGER
        {
        wavelength_1310nm(1),
        wavelength_1550nm(2)
        }

    RxActivityMode ::= INTEGER
        {
        alwaysActive(1),
        forceSleep(2),
        automatic(3)
        }

    RxRfOutputLevelRangeMode ::= INTEGER
        {
        defaultUsingDeviceSpecs(1),
        extendedInputPowerBased(2)
        }

    RxActivityValue ::= INTEGER
        {
        active(1),
        sleeping(2)
        }

    RxOutputSelectValue ::= INTEGER
        {
        oneOutput(1),
        twoOutputs(2)
        }

    RxInactivityBehaviourValue ::= INTEGER
        {
        sleepWhenInactive(1),
        hotStandbyWhenInactive(2)
        }

    RxPilotPskUsageValue ::= INTEGER
        {
        enableLinecodes(1),
        disableAll(2),
        enableLinecodesAndRemoteStatus(3)
        }

    RxLineCodeMismatchActionValue ::= INTEGER
        {
        noLineCodeWarningOrNotify(1),
        notifyOnCodeMismatch(2),
        warningOnCodeMismatch(3)
        }

    TxPilotPskUsageValue ::= INTEGER
        {
        enablePskCoding(1),
        disablePskCoding(2)
        }

    LineCodeNumberValue ::= INTEGER (0..65535)

    NESlotWriteValue ::= INTEGER (-1..99)


