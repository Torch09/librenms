        otrxStatesTx1OutputPowerLow      PerceivedSeverityValue,
        otrxStatesTx1OutputPowerHigh     PerceivedSeverityValue,
        otrxStatesTx2OutputPowerLow      PerceivedSeverityValue,
        otrxStatesTx2OutputPowerHigh     PerceivedSeverityValue,
        otrxStatesTx1LaserCurrentLow     PerceivedSeverityValue,
        otrxStatesTx1LaserCurrentHigh    PerceivedSeverityValue,
        otrxStatesTx2LaserCurrentLow     PerceivedSeverityValue,
        otrxStatesTx2LaserCurrentHigh    PerceivedSeverityValue,
        otrxStatesTx1LaserShutdown       PerceivedSeverityValue,
        otrxStatesTx2LaserShutdown       PerceivedSeverityValue,
        otrxStatesInputVoltageLow        PerceivedSeverityValue,
        otrxStatesInputVoltageHigh       PerceivedSeverityValue,
        otrxStatesInternalVoltageLow     PerceivedSeverityValue,
        otrxStatesInternalVoltageHigh    PerceivedSeverityValue,
        otrxStatesRxRegulatorLimit       PerceivedSeverityValue,
        otrxStatesLidOpen                PerceivedSeverityValue,
        otrxStatesSlotDetection          PerceivedSeverityValue,
        otrxStatesTx1PilotOmiLow         PerceivedSeverityValue,  -- For future use only
        otrxStatesTx1PilotOmiHigh        PerceivedSeverityValue,  -- For future use only
        otrxStatesTx2PilotOmiLow         PerceivedSeverityValue,  -- For future use only
        otrxStatesTx2PilotOmiHigh        PerceivedSeverityValue,  -- For future use only
        otrxStatesRxRfInputPowerRmsLow   PerceivedSeverityValue,  -- For future use only
        otrxStatesRxRfInputPowerRmsHigh  PerceivedSeverityValue,  -- For future use only
        otrxStatesTx1RegulatorLimit      PerceivedSeverityValue,
        otrxStatesTx2RegulatorLimit      PerceivedSeverityValue,
        otrxStatesBkRackExtPowerSupply   PerceivedSeverityValue,
        otrxStatesRxLineCodeNumber       PerceivedSeverityValue,
        otrxStatesRxPskRemoteState       PerceivedSeverityValue,
        otrxStatesRxPilotSignal          PerceivedSeverityValue
