--*****************************************************************************************
--  otrxControl group

    otrxControlTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrxControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otrxControl 1 }

-- table of control entry(ro/rw)

    otrxControlEntry OBJECT-TYPE
        SYNTAX OtrxControlEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { otrxNESlot }
    ::= { otrxControlTable 1 }

-- the structure of the entry

    OtrxControlEntry ::= SEQUENCE
    {
        otrxControlReset            TruthValue,
        otrxControlModuleLedBlink   TruthValue
    }

    otrxControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { otrxControlEntry 1 }

    otrxControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { otrxControlEntry 2 }

--*****************************************************************************************
-- otrxConfiguration group

    otrxConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrxConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otrxConfiguration 1 }

-- table of configuration entry(ro/rw)

    otrxConfigurationEntry OBJECT-TYPE
        SYNTAX OtrxConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { otrxNESlot  }
    ::= { otrxConfigurationTable 1 }

-- the structure of the entry

    OtrxConfigurationEntry ::= SEQUENCE
    {
        otrxConfigNESlotWrite                      NESlotWriteValue,
        otrxConfigTx1RfGain                        INTEGER,
        otrxConfigTx2RfGain                        INTEGER,
        otrxConfigTx1OutputLevel                   INTEGER,
        otrxConfigTx2OutputLevel                   INTEGER,
        otrxConfigTx1ActivityMode                  TxActivityMode,
        otrxConfigTx2ActivityMode                  TxActivityMode,
        otrxConfigTx1IngressMode                   TxIngressMode,
        otrxConfigTx2IngressMode                   TxIngressMode,
        otrxConfigTx1PilotPskUsage                 TxPilotPskUsageValue,
        otrxConfigTx2PilotPskUsage                 TxPilotPskUsageValue,
        otrxConfigTx1LineCodeNumber                LineCodeNumberValue,
        otrxConfigTx2LineCodeNumber                LineCodeNumberValue,
        otrxConfigTx1PilotActivity                 TxPilotActivityValue,
        otrxConfigTx2PilotActivity                 TxPilotActivityValue,
        otrxConfigTx1PilotOmi                      INTEGER,
        otrxConfigTx2PilotOmi                      INTEGER,
        otrxConfigRxInputWavelength                RxInputWavelengthValue,
        otrxConfigRxRfOutputLevel                  INTEGER,
        otrxConfigRxRfAttenuatorManual             INTEGER,
        otrxConfigRxActivityMode                   RxActivityMode,
        otrxConfigRxRegulationMode                 RxRegulationMode,
        otrxConfigRxRfOutputLevelRangeMode         RxRfOutputLevelRangeMode,
        otrxConfigRedundancySetting                RedundancySettingValue,
        otrxConfigNominalRedundantMode             NominalRedundantMode,
        otrxConfigBkRackLidNotifyMode              BkRackLidNotifyMode,
        otrxConfigBkRackExtPowerSupplyAlarmMode    BkRackExtPowerSupplyAlarmMode,
        otrxConfigRxOutputSelect                   RxOutputSelectValue,
        otrxConfigRxInactivityBehaviour            RxInactivityBehaviourValue,
        otrxConfigRxPilotPskUsage                  RxPilotPskUsageValue,
        otrxConfigRxLineCodeMismatchAction         RxLineCodeMismatchActionValue,
        otrxConfigRxExpectedLineCodeNumber         LineCodeNumberValue
    }

    otrxConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
   ::= { otrxConfigurationEntry 1 }

    otrxConfigTx1RfGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF Gain in steps of 0.1dB (Transmitter #1)"
    ::= { otrxConfigurationEntry 2 }

    otrxConfigTx2RfGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF Gain in steps of 0.1dB (Transmitter #2)"
    ::= { otrxConfigurationEntry 3 }

    otrxConfigTx1OutputLevel OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Optical output power level in steps of 0.1dB,
                     relative to nominal power (Transmitter #1),
                     see otrxDisplayTx1OutputPowerNominal"
    ::= { otrxConfigurationEntry 4 }

    otrxConfigTx2OutputLevel OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Optical output power level in steps of 0.1dB,
                     relative to nominal power (Transmitter #2),
                     see otrxDisplayTx2OutputPowerNominal"
    ::= { otrxConfigurationEntry 5 }

    otrxConfigTx1ActivityMode OBJECT-TYPE
        SYNTAX TxActivityMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Activity settings (Transmitter #1)"
    ::= { otrxConfigurationEntry 6 }

    otrxConfigTx2ActivityMode OBJECT-TYPE
        SYNTAX TxActivityMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Activity settings (Transmitter #2)"
    ::= { otrxConfigurationEntry 7 }

    otrxConfigTx1IngressMode OBJECT-TYPE
        SYNTAX TxIngressMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Ingress settings (Transmitter #1)"
    ::= { otrxConfigurationEntry 8 }

    otrxConfigTx2IngressMode OBJECT-TYPE
        SYNTAX TxIngressMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Ingress settings (Transmitter #2)"
    ::= { otrxConfigurationEntry 9 }

    otrxConfigTx1PilotPskUsage OBJECT-TYPE
        SYNTAX TxPilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "PSK pilot coding settings (Transmitter #1)"
    ::= { otrxConfigurationEntry 10 }

    otrxConfigTx2PilotPskUsage OBJECT-TYPE
        SYNTAX TxPilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "PSK pilot coding settings (Transmitter #2)"
    ::= { otrxConfigurationEntry 11 }

    otrxConfigTx1LineCodeNumber OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Linecode number to be sent (Transmitter #1)"
    ::= { otrxConfigurationEntry 12 }

    otrxConfigTx2LineCodeNumber OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Linecode number to be sent (Transmitter #2)"
    ::= { otrxConfigurationEntry 13 }

    otrxConfigTx1PilotActivity OBJECT-TYPE
        SYNTAX TxPilotActivityValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Pilot on/off settings (Transmitter #1)"
    ::= { otrxConfigurationEntry 14 }

    otrxConfigTx2PilotActivity OBJECT-TYPE
        SYNTAX TxPilotActivityValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Pilot on/off settings (Transmitter #2)"
    ::= { otrxConfigurationEntry 15 }

    otrxConfigTx1PilotOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Optical modulation index of pilot signal in steps of 0.1dB,
                     relative to nominal PilotOMI (Transmitter #1),
                     see otrxDisplayTx1PilotOmiNominal"
    ::= { otrxConfigurationEntry 16 }

    otrxConfigTx2PilotOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Optical modulation index of pilot signal in steps of 0.1dB,
                     relative to nominal PilotOMI (Transmitter #2),
                     see otrxDisplayTx2PilotOmiNominal"
    ::= { otrxConfigurationEntry 17 }

    otrxConfigRxInputWavelength OBJECT-TYPE
        SYNTAX RxInputWavelengthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Optical input wavelength (Receiver).
                     (Used to adjust input power measurement)"
    ::= { otrxConfigurationEntry 18 }

    otrxConfigRxRfOutputLevel OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF output level in steps of 0.1dB, relative to nominal value (Receiver),
                     see otrxDisplayRxRfOutputLevelNominal.
                     Only usable if otrxConfigRxRegulationMode == inputPowerBasedRegulation(2)
                     or otrxConfigRxRegulationMode == pilotBasedRegulation(3)"
    ::= { otrxConfigurationEntry 19 }

    otrxConfigRxRfAttenuatorManual OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "RF attenuator setting in steps of 0.1dB (Receiver).
                     Only usable if otrxConfigRxRegulationMode == noRegulationManualSetup(1)"
    ::= { otrxConfigurationEntry 20 }

    otrxConfigRxActivityMode OBJECT-TYPE
        SYNTAX RxActivityMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Activity settings (Receiver)"
    ::= { otrxConfigurationEntry 21 }

    otrxConfigRxRegulationMode OBJECT-TYPE
        SYNTAX RxRegulationMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The regulation mode (Receiver)
                     Note that pilotBasedRegulation(3) is not supported by all devices,
                     see see otrxDisplayRxPilotBasedRegulationSupported"
    ::= { otrxConfigurationEntry 22 }

    otrxConfigRxRfOutputLevelRangeMode OBJECT-TYPE
        SYNTAX RxRfOutputLevelRangeMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Settings for allowed range of RF output level (Receiver)."
    ::= { otrxConfigurationEntry 23 }

    otrxConfigRedundancySetting OBJECT-TYPE
        SYNTAX RedundancySettingValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Redundancy settings."
    ::= { otrxConfigurationEntry 24 }

    otrxConfigNominalRedundantMode OBJECT-TYPE
        SYNTAX NominalRedundantMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Device nominal or redundant mode setup."
    ::= { otrxConfigurationEntry 25 }

    otrxConfigBkRackLidNotifyMode OBJECT-TYPE
        SYNTAX BkRackLidNotifyMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The kind of notification the device should rise,
                     when lid of bk-rack gets opened."
    ::= { otrxConfigurationEntry 26 }

    otrxConfigBkRackExtPowerSupplyAlarmMode OBJECT-TYPE
        SYNTAX BkRackExtPowerSupplyAlarmMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The kind of event notification the device should rise,
            when external power supply notifies a failure via BK-rack pin7."
    ::= { otrxConfigurationEntry 27 }

    otrxConfigRxOutputSelect OBJECT-TYPE
        SYNTAX RxOutputSelectValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The number of outputs the receiver uses.
                     Not supported by all devices, see otrxDisplayRxOutputSelectSupported
                     Device that do not support always report oneOutput(1)"
    ::= { otrxConfigurationEntry 28 }

    otrxConfigRxInactivityBehaviour OBJECT-TYPE
        SYNTAX RxInactivityBehaviourValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The receiver's behaviour in inactive or redundant states
                     Not adjustable by all devices, see otrxDisplayRxInactivityBehaviourSupported
                     Device that do not support, only report their inactivity behaviour"
    ::= { otrxConfigurationEntry 29 }

    otrxConfigRxPilotPskUsage OBJECT-TYPE
        SYNTAX RxPilotPskUsageValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "PSK pilot coding settings (Receiver)
                     Not supported by all devices:
                     if otrxDisplayRxPilotPskSupported       == false(2)
                        -> only disableAll(1) is allowed
                     if otrxDisplayRxPskRemoteStateSupported == false(2)
                        -> enableRxLinecodesAndRemoteStatus(3) is not allowed"
    ::= { otrxConfigurationEntry 30 }

    otrxConfigRxLineCodeMismatchAction OBJECT-TYPE
        SYNTAX RxLineCodeMismatchActionValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The action that has to occur on a linecode mismatch between received
                     and expected linecodes or on a PSK remote signal loss detection.
                     Not supported by all devices, see otrxDisplayRxPilotPskSupported"
    ::= { otrxConfigurationEntry 31 }

    otrxConfigRxExpectedLineCodeNumber OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The expected remote transmitter's linecode number
                     Not supported by all devices, see otrxDisplayRxPilotPskSupported"
    ::= { otrxConfigurationEntry 32 }


