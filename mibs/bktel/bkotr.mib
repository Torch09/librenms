--*****************************************************************************************
--  otrStates group

-- Note: For every control OId there must be a correspondant alarm OId!

    otrStatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otrStates 1 }

-- table of states entry(ro/rw)

    otrStatesEntry OBJECT-TYPE
        SYNTAX OtrStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { otrNESlot }
    ::= { otrStatesTable 1 }

-- the structure of the entry

    OtrStatesEntry ::= SEQUENCE
    {
        otrStatesInputLow            PerceivedSeverityValue,
        otrStatesInputHigh           PerceivedSeverityValue,
        otrStatesOutputLow           PerceivedSeverityValue,
        otrStatesOutputHigh          PerceivedSeverityValue,
        otrStatesLaserAging          PerceivedSeverityValue,
        otrStatesPlus24VLow          PerceivedSeverityValue,
        otrStatesPlus24VHigh         PerceivedSeverityValue,
        otrStatesTemperatureLow      PerceivedSeverityValue,
        otrStatesTemperatureHigh     PerceivedSeverityValue,
        otrStatesLidOpen             PerceivedSeverityValue,
        otrStatesSwitchedToRedundant PerceivedSeverityValue,
        otrStatesTxPilotFailure      PerceivedSeverityValue,
        otrStatesTxShutdown          PerceivedSeverityValue,
        otrStatesBootloader          PerceivedSeverityValue,
        otrStatesDefective           PerceivedSeverityValue
    }

    otrStatesInputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical input power low"
    ::= { otrStatesEntry 1 }

    otrStatesInputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical input power high"
    ::= { otrStatesEntry 2 }

    otrStatesOutputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical output power low"
    ::= { otrStatesEntry 3 }

    otrStatesOutputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical output power high"
    ::= { otrStatesEntry 4 }

    otrStatesLaserAging OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The laser is aging"
    ::= { otrStatesEntry 5 }

    otrStatesPlus24VLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "24 Volt supply low"
    ::= { otrStatesEntry 6 }

    otrStatesPlus24VHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "24 Volt supply high"
    ::= { otrStatesEntry 7 }

    otrStatesTemperatureLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature low"
    ::= { otrStatesEntry 8 }

    otrStatesTemperatureHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device internal temperature high"
    ::= { otrStatesEntry 9 }

    otrStatesLidOpen OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The lid of the BK rack is open"
    ::= { otrStatesEntry 10 }

    otrStatesSwitchedToRedundant OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The device has executed a redundancy switch"
    ::= { otrStatesEntry 11 }

    otrStatesTxPilotFailure OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Pilot failure"
    ::= { otrStatesEntry 12 }

    otrStatesTxShutdown OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Transmitter shutdown by -6dB"
    ::= { otrStatesEntry 13 }

    otrStatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION 
            "The device is running in bootloader mode 
             without a legal application software." 
    ::= { otrStatesEntry 14 }

    otrStatesDefective OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device. 
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not 
             by the device" 
    ::= { otrStatesEntry 15 }

--*****************************************************************************************
--  otrControl group

  otrControlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF OtrControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
  ::= { otrControl 1 }

-- table of control entry(ro/rw)

  otrControlEntry OBJECT-TYPE
    SYNTAX OtrControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "Alarms for a module."
    INDEX { otrNESlot }
  ::= { otrControlTable 1 }

-- the structure of the entry

    OtrControlEntry ::= SEQUENCE
    {
        otrControlReset               TruthValue,
        otrControlModuleLedBlink      TruthValue,
        otrControlLaserShutdown       TruthValue
    }

    otrControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { otrControlEntry 1 }

    otrControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "Writing this variable to true(1) lets the device's 
        modul LED blink green for 10 seconds. 
        Writing this variable to false(2) stotr blinking at once.
        This variable always returns false(2) on read requests"
    ::= { otrControlEntry 2 }

    otrControlLaserShutdown OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Allows to shutdown the transmitter (Laser output power minus 6dB)."
    ::= { otrControlEntry 3 }


--*****************************************************************************************
-- otrConfiguration group

    otrConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otrConfiguration 1 }

-- table of configuration entry(ro/rw)

    otrConfigurationEntry OBJECT-TYPE
        SYNTAX OtrConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { otrNESlot  }
    ::= { otrConfigurationTable 1 }

-- the structure of the entry

    OtrConfigurationEntry ::= SEQUENCE
    {
        -- Common
        otrConfigNESlotWrite             NESlotWriteValue,
        otrConfigLedActivity             LedActivityValue,
        -- Receiver
        otrConfigInputPwrLimitLoLo       INTEGER,
        otrConfigInputPwrLimitLo         INTEGER,
        otrConfigInputPwrLimitHi         INTEGER,
        otrConfigInputPwrLimitHiHi       INTEGER,
        otrConfigRfOutDcSettingsEnable   TruthValue,
        otrConfigAuxRfOutputMode         AuxRfOutputModeValue,        
        -- Transmitter
        otrConfigAuxRfInputMode          AuxRfInputModeValue,
        otrConfigOmi                     INTEGER,
        otrConfigSendPilotSignal         TruthValue,
        otrConfigLineCodeEnable          TruthValue,
        otrConfigLineCodeNumber          LineCodeNumberValue,
        otrConfigLineCodeNomRedFlag      LineCodeNomRedFlagValue,
        -- Receiver
        otrConfigOutputVoltageOffset     INTEGER
    }

-- Common part
    
    otrConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { otrConfigurationEntry 1 }

    otrConfigLedActivity OBJECT-TYPE
        SYNTAX LedActivityValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION 
            "Set the activity mode of the device LED's.   
             Can be used to save some power in fibernodes."
    ::= { otrConfigurationEntry 2 }

-- Receiver part
    
    otrConfigInputPwrLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power low alarm limit in 0.1 dBm units"
    ::={ otrConfigurationEntry 3 }

    otrConfigInputPwrLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power low warning limit in 0.1 dBm units"
    ::={ otrConfigurationEntry 4 }

    otrConfigInputPwrLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power high warning limit in 0.1 dBm units"
    ::={ otrConfigurationEntry 5 }

    otrConfigInputPwrLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power high alarm limit in 0.1 dBm units"
    ::={ otrConfigurationEntry 6 }

    otrConfigRfOutDcSettingsEnable OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The OTR can be DC remote controlled by the rf-module that
             follows the optical receiver's main rf-output for redundancy 
             purposes.
             Allows to enable or disable this DC remote control feature."
    ::={ otrConfigurationEntry 7 }

    otrConfigAuxRfOutputMode OBJECT-TYPE
        SYNTAX  AuxRfOutputModeValue
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The mode of the receivers auxiliary rf output.
             Note that dcRemoteControlled(1) is a possible value 
             only if RfOutDcSettings is enabled"
    ::={ otrConfigurationEntry 8 }

-- Transmitter part

    otrConfigAuxRfInputMode OBJECT-TYPE
        SYNTAX  AuxRfInputModeValue
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION "The mode of the transmitters auxiliary rf input"
    ::={ otrConfigurationEntry 9 }

    otrConfigOmi OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The optical modulation index in 0.1 percent units."
    ::= { otrConfigurationEntry 10 }

    otrConfigSendPilotSignal OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Enable/disable transmitter's sending of pilot signal."
    ::= { otrConfigurationEntry 11 }

    otrConfigLineCodeEnable OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Enable/disable transmitter's sending of line code signals."
    ::= { otrConfigurationEntry 12 }

    otrConfigLineCodeNumber OBJECT-TYPE
        SYNTAX LineCodeNumberValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION 
            "The line code number that the transmitter shall send.
             This line code number can be used on the remote side 
             optical receiver to identify the connected transmitter."
    ::= { otrConfigurationEntry 13 }

    otrConfigLineCodeNomRedFlag OBJECT-TYPE
        SYNTAX LineCodeNomRedFlagValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION 
            "The line code nominal/redundant flag that the transmitter shall send.
             This flag informs the remote optical receiver (orx2) if this OTR device 
             acts as nominal or as redundant device."
    ::= { otrConfigurationEntry 14 }

    otrConfigOutputVoltageOffset OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The offset from nominal output level voltage in 0.1 dB units"
    ::={ otrConfigurationEntry 15 }



--*****************************************************************************************
-- Display group

    otrDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { otrDisplay 1 }

-- table of configuration entry(ro/rw)

    otrDisplayEntry OBJECT-TYPE
        SYNTAX OtrDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { otrNESlot  }
    ::= { otrDisplayTable 1 }

-- the structure of the entry

    OtrDisplayEntry ::= SEQUENCE
    {
        otrDisplayRxActive           TruthValue,
        otrDisplayRxAuxOutputActive  TruthValue,
        otrDisplayTxAuxInputActive   TruthValue
    }

    otrDisplayRxActive OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION 
            "The activity state of the optical receiver. 
             If not active the complete receiver sleeps with low power consumption."
      ::= { otrDisplayEntry 1 }

    otrDisplayRxAuxOutputActive OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The auxiliary rf output activity state of the optical receiver."
      ::= { otrDisplayEntry 2 }

    otrDisplayTxAuxInputActive OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The auxiliary rf input activity state of the optical transmitter."
      ::= { otrDisplayEntry 3 }



--*****************************************************************************************
END
