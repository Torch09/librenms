BKTEL-HFC862-OTR-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    otr                 OBJECT IDENTIFIER ::= { modules 107 }
    otrCommon           OBJECT IDENTIFIER ::= { otr 1 }
    otrStates           OBJECT IDENTIFIER ::= { otr 2 }
    otrConfiguration    OBJECT IDENTIFIER ::= { otr 3 }
    otrControl          OBJECT IDENTIFIER ::= { otr 4 }
    otrMeasuringValues  OBJECT IDENTIFIER ::= { otr 5 }
    otrDisplay          OBJECT IDENTIFIER ::= { otr 6 }


--
-- Type definitions
--

        NESlotWriteValue ::= INTEGER (-1..99)

        LedActivityValue ::= INTEGER
            {
            ledsAlwaysActive(1),
            ledsActiveOnlyIfLidOpen(2)
            }

        AuxRfOutputModeValue ::= INTEGER
            {
            dcRemoteControlled(1),
            alwaysOn(2),
            alwaysOff(3)
            }

        AuxRfInputModeValue ::= INTEGER
            {
            followReceiversAuxOutput(1),
            alwaysOn(2),
            alwaysOff(3)
            }

        OptOutputModeValue ::= INTEGER
            {
            optOutputNominal(1),
            optOutputSleep(2)
            }

        LineCodeNumberValue ::= INTEGER (0..999)

        LineCodeNomRedFlagValue ::= INTEGER
            {
            sendNominalFlag(1),
            sendRedundantFlag(2)
            }


--*****************************************************************************************
-- OTR (Optical Transceiver (BOX860) design release V01)

--*****************************************************************************************
-- otrCommon group

    otrCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
  ::= { otrCommon 1 }

    otrCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OTR-type in the NE"
  ::= { otrCommon 2 }

-- table of states entry(ro/rw)

    otrCommonEntry OBJECT-TYPE
        SYNTAX OtrCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { otrNESlot }
  ::= { otrCommonTable 1 }

-- the structure of the entry

    OtrCommonEntry ::= SEQUENCE
    {
        otrNESlot                      NESlotValue,
        otrCommonType                  DisplayString,
        otrCommonDescr                 DisplayString,
        otrCommonFirmwareId            DisplayString,
        otrCommonModuleWidth           ModuleWidthValue
    }

    otrNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { otrCommonEntry 1 }

    otrCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { otrCommonEntry 2 }

    otrCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { otrCommonEntry 3 }

    otrCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { otrCommonEntry 4 }

    otrCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= { otrCommonEntry 5 }

--*****************************************************************************************
-- otrMeasuringValues group

-- table of measuring Values

    otrMeasuringValuesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OtrMeasuringValuesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
   ::= { otrMeasuringValues 1 }

-- table of measuring Values entry

    otrMeasuringValuesEntry OBJECT-TYPE
        SYNTAX OtrMeasuringValuesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { otrNESlot }
  ::= { otrMeasuringValuesTable 1 }

--  the structure of the entry

    OtrMeasuringValuesEntry ::= SEQUENCE
    {
        otrInputPowerLoLo             INTEGER,
        otrInputPowerLo               INTEGER,
        otrInputPowerValue            INTEGER,
        otrInputPowerHi               INTEGER,
        otrInputPowerHiHi             INTEGER,

        otrOutputPowerLoLo            INTEGER,
        otrOutputPowerLo              INTEGER,
        otrOutputPowerValue           INTEGER,
        otrOutputPowerHi              INTEGER,
        otrOutputPowerHiHi            INTEGER,

        otrLaserCurrentRelLoLo        INTEGER,
        otrLaserCurrentRelLo          INTEGER,
        otrLaserCurrentRelValue       INTEGER,
        otrLaserCurrentRelHi          INTEGER,
        otrLaserCurrentRelHiHi        INTEGER,

        otrPlus24VLoLo                INTEGER,
        otrPlus24VLo                  INTEGER,
        otrPlus24VValue               INTEGER,
        otrPlus24VHi                  INTEGER,
        otrPlus24VHiHi                INTEGER,

        otrTemperatureLoLo            INTEGER,
        otrTemperatureLo              INTEGER,
        otrTemperatureValue           INTEGER,
        otrTemperatureHi              INTEGER,
        otrTemperatureHiHi            INTEGER
    }

-- *****
    otrInputPowerLoLo OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input power low alarm threshold in steps of 0.1dBm."
