BKTEL-HFC862-ORX4-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    orx4                 OBJECT IDENTIFIER ::= { modules 113 }
    orx4Common           OBJECT IDENTIFIER ::= { orx4 1 }
    orx4States           OBJECT IDENTIFIER ::= { orx4 2 }
    orx4Configuration    OBJECT IDENTIFIER ::= { orx4 3 }
    orx4Control          OBJECT IDENTIFIER ::= { orx4 4 }
    orx4MeasuringValues  OBJECT IDENTIFIER ::= { orx4 5 }
    orx4Display          OBJECT IDENTIFIER ::= { orx4 6 }


--
-- Type definitions
--

    TestOutputValue ::= INTEGER
        {
        testOutputIsolate(0),
        testOutputChannel_1(1),
        testOutputChannel_2(2),
        testOutputChannel_3(3),
        testOutputChannel_4(4)
        }

    PilotPskUsageValue ::= INTEGER
        {
        enableLinecodes(1),
        disableAll(2),
        enableLinecodesAndRemoteStatus(3)
        }

    LineCodeMismatchActionValue ::= INTEGER
        {
        noLineCodeWarningOrNotify(1),
        notifyOnCodeMismatch(2),
        warningOnCodeMismatch(3)
        }

    RedundancySettingValue ::= INTEGER
        {
        noRedundancy(1),
        deviceRedundancy(2),
        channelRedundancy(3),
        channelRedundancyCh1Ch2Only(4)
        }

    OutputOnChannelRedundancyValue ::= INTEGER
        {
        separateOutputs(1),
        commonOutput(2)
        }

    DeviceRedundancyDetectionValue ::= INTEGER
        {
        automaticBySlotNumber(1),
        manualBySetup(2)
        }

    DeviceNominalRedundantValue ::= INTEGER
        {
        nominalDevice(1),
        redundantDevice(2)
        }

    ActivityMode ::= INTEGER
        {
        alwaysActive(1),
        forceSleepOrStandby(2),
        automatic(3),
        deactivate(4)
        }

    InactivityBehaviourValue ::= INTEGER
        {
        sleepWhenInactive(1),
        hotStandbyWhenInactive(2)
        }

    RegulationModeValue ::= INTEGER
        {
        noRegulationManualSetup(1),
        inputPowerBasedRegulation(2),
        pilotBasedRegulation(3)
        }

    InputWavelengthValue ::= INTEGER
        {
        wavelength_1310nm(1),
        wavelength_1550nm(2)
        }

    RfOutputLevelRangeMode ::= INTEGER
        {
        defaultUsingDeviceSpecs(1),
        extendedInputPowerBased(2)
        }

    LineCodeNumberValue ::= INTEGER (0..65535)

    ActivityValue ::= INTEGER
        {
        active(1),
        sleeping(2),
        standby(3),
        deactivated(4),
        notAssembled(5)
        }

    NESlotWriteValue ::= INTEGER (-1..99)


