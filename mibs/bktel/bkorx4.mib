--*****************************************************************************************
--  orx4Control group

  orx4ControlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Orx4ControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
  ::= { orx4Control 1 }

-- table of control entry(ro/rw)

  orx4ControlEntry OBJECT-TYPE
    SYNTAX Orx4ControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "Alarms for a module."
    INDEX { orx4NESlot }
  ::= { orx4ControlTable 1 }

-- the structure of the entry

    Orx4ControlEntry ::= SEQUENCE
    {
        orx4ControlReset             TruthValue,
        orx4ControlModuleLedBlink    TruthValue
    }

    orx4ControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { orx4ControlEntry 1 }

    orx4ControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) storx4 blinking at once.
        This variable always returns false(2) on read requests"
    ::= { orx4ControlEntry 2 }


