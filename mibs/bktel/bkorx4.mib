-- $idx$: _Chn
-- MibCompiler defs --


--*****************************************************************************************
-- ORX4 ( Optical Receiver with a maximum number of four optical inputs )


--*****************************************************************************************
-- orx4Common group

    orx4CommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
  ::= { orx4Common 1 }

    orx4CommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF Orx4CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the ORX4-type in the NE"
  ::= { orx4Common 2 }

-- table of states entry(ro/rw)

    orx4CommonEntry OBJECT-TYPE
        SYNTAX Orx4CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { orx4NESlot }
  ::= { orx4CommonTable 1 }

-- the structure of the entry

    Orx4CommonEntry ::= SEQUENCE
    {
        orx4NESlot                      NESlotValue,
        orx4CommonType                  DisplayString,
        orx4CommonDescr                 DisplayString,
        orx4CommonFirmwareId            DisplayString,
        orx4CommonModuleWidth           ModuleWidthValue
    }

    orx4NESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { orx4CommonEntry 1 }

    orx4CommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { orx4CommonEntry 2 }

    orx4CommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { orx4CommonEntry 3 }

    orx4CommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { orx4CommonEntry 4 }

    orx4CommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= { orx4CommonEntry 5 }

