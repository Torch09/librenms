--*****************************************************************************************
--  ovaV01States group

-- Note: For every control OId there must be a correspondant alarm OId!

    ovaV01StatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaV01StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ovaV01States 1 }

-- table of states entry(ro/rw)

    ovaV01StatesEntry OBJECT-TYPE
        SYNTAX OvaV01StatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { ovaV01NESlot }
    ::= { ovaV01StatesTable 1 }

-- the structure of the entry

    OvaV01StatesEntry ::= SEQUENCE
    {
        ovaV01StatesSystem             PerceivedSeverityValue,
        ovaV01StatesOutputLow          PerceivedSeverityValue,
        ovaV01StatesOutputHigh         PerceivedSeverityValue,
        ovaV01StatesInputLow           PerceivedSeverityValue,
        ovaV01StatesInputHigh          PerceivedSeverityValue,
        ovaV01StatesLaserAging         PerceivedSeverityValue,
        ovaV01StatesTecHigh            PerceivedSeverityValue,
        ovaV01StatesLaserShutdown      PerceivedSeverityValue,
        ovaV01StatesInitializing       PerceivedSeverityValue,
        ovaV01StatesBootloader         PerceivedSeverityValue,
        ovaV01StatesDefective          PerceivedSeverityValue
    }

    ovaV01StatesSystem OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Internal error occured"
    ::= { ovaV01StatesEntry 1 }

    ovaV01StatesOutputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical output power low"
    ::= { ovaV01StatesEntry 2 }

    ovaV01StatesOutputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical output power high"
    ::= { ovaV01StatesEntry 3 }

    ovaV01StatesInputLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical input power low"
    ::= { ovaV01StatesEntry 4 }

    ovaV01StatesInputHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Optical input power high"
    ::= { ovaV01StatesEntry 5 }

    ovaV01StatesLaserAging OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The laser is aging"
    ::= { ovaV01StatesEntry 6 }

    ovaV01StatesTecHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Laser cooler limit reached"
    ::= { ovaV01StatesEntry 7 }

    ovaV01StatesLaserShutdown OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Laser is shutdown, no optical output"
    ::= { ovaV01StatesEntry 8 }

    ovaV01StatesInitializing OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The device is initializing"
    ::= { ovaV01StatesEntry 9 }

    ovaV01StatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION 
            "The device is running in bootloader mode 
             without a legal application software." 
    ::= { ovaV01StatesEntry 10 }

    ovaV01StatesDefective OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device. 
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not 
             by the device" 
    ::= { ovaV01StatesEntry 11 }


--*****************************************************************************************
--  ovaV01Control group

  ovaV01ControlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF OvaV01ControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
  ::= { ovaV01Control 1 }

-- table of control entry(ro/rw)

  ovaV01ControlEntry OBJECT-TYPE
    SYNTAX OvaV01ControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "Alarms for a module."
    INDEX { ovaV01NESlot }
  ::= { ovaV01ControlTable 1 }

-- the structure of the entry

    OvaV01ControlEntry ::= SEQUENCE
    {
        ovaV01ControlLaserShutdown                     TruthValue,
        ovaV01ControlReset                             TruthValue,
        ovaV01ControlModuleLedBlink                    TruthValue
    }

    ovaV01ControlLaserShutdown OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Shutdown the laser(s), optical power on/off."
    ::= { ovaV01ControlEntry 1 }

    ovaV01ControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { ovaV01ControlEntry 2 }

    ovaV01ControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "Writing this variable to true(1) lets the device's 
        modul LED blink green for 10 seconds. 
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { ovaV01ControlEntry 3 }


--*****************************************************************************************
-- ovaV01Configuration group

    ovaV01ConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaV01ConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ovaV01Configuration 1 }

-- table of configuration entry(ro/rw)

    ovaV01ConfigurationEntry OBJECT-TYPE
        SYNTAX OvaV01ConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ovaV01NESlot  }
    ::= { ovaV01ConfigurationTable 1 }

-- the structure of the entry

    OvaV01ConfigurationEntry ::= SEQUENCE
    {
        ovaV01ConfigNESlotWrite                       NESlotWriteValue,
        ovaV01ConfigOutputPwrOrGainAdjust             INTEGER,
        ovaV01ConfigInputPwrLimitLo                   INTEGER
    }

    ovaV01ConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { ovaV01ConfigurationEntry 1 }

    ovaV01ConfigOutputPwrOrGainAdjust OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This variable can be used to adjust the desired output power (or gain) 
             value in 0.1 dB(m) units.
             See ovaV01DisplayRegulationMode variable too"
    ::={ ovaV01ConfigurationEntry 2 }

    ovaV01ConfigInputPwrLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power low warning limit in 0.1 dBm units"
    ::={ ovaV01ConfigurationEntry 3 }


--*****************************************************************************************
-- Display group

    ovaV01DisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaV01DisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ovaV01Display 1 }

-- table of configuration entry(ro/rw)

    ovaV01DisplayEntry OBJECT-TYPE
        SYNTAX OvaV01DisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ovaV01NESlot  }
    ::= { ovaV01DisplayTable 1 }

-- the structure of the entry

    OvaV01DisplayEntry ::= SEQUENCE
    {
        ovaV01DisplayLaserCount                INTEGER,
        ovaV01DisplayOutputPwrOrGainAdjustMin  INTEGER,
        ovaV01DisplayOutputPwrOrGainAdjustMax  INTEGER,
        ovaV01DisplayOutputPwrOrGainAdjustStep INTEGER,
        ovaV01DisplayLaser1PumpPowerRel        INTEGER,
        ovaV01DisplayLaser2PumpPowerRel        INTEGER,
        ovaV01DisplayRegulationMode            RegulationMode
    }

    ovaV01DisplayLaserCount OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The number of lasers used by this optical amplifier"
    ::={ ovaV01DisplayEntry 1 }

    ovaV01DisplayOutputPwrOrGainAdjustMin OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The maximum adjustable output power (or gain) value in 0.1 dB(m) units.
             See ovaV01DisplayRegulationMode variable too"
    ::={ ovaV01DisplayEntry 2 }

    ovaV01DisplayOutputPwrOrGainAdjustMax OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The minimum adjustable output power (or gain) value in 0.1 dB(m) units.
             See ovaV01DisplayRegulationMode variable too"
    ::={ ovaV01DisplayEntry 3 }

    ovaV01DisplayOutputPwrOrGainAdjustStep OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The stepsize that can be used to adjust the output power (or gain)
             value in 0.1 dB units.
             See ovaV01DisplayRegulationMode variable too"
    ::={ ovaV01DisplayEntry 4 }

    ovaV01DisplayLaser1PumpPowerRel OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The pump power of laser no.1 in percent"
    ::={ ovaV01DisplayEntry 5 }

    ovaV01DisplayLaser2PumpPowerRel OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The pump power of laser no.2 in percent"
    ::={ ovaV01DisplayEntry 6 }

    ovaV01DisplayRegulationMode OBJECT-TYPE
        SYNTAX  RegulationMode
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The regulation mode of the device.
             - regModeConstantOutputPower(1):
               If the device runs in constant output power mode, then all *OutputPwrOrGain*
               variables are interpreted as 'Output Power' specific.
             - regModeConstantGain(2):
               If the device runs in constant gain mode, then all *OutputPwrOrGain*
               variables are interpreted as 'Gain' specific."
    ::={ ovaV01DisplayEntry 7 }


--*****************************************************************************************
END
