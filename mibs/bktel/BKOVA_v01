--- ****************************************************************************
-- *
-- * Description:   Implements definitions for the OvaV01-modul
-- *                (Optical Amplifier [old type] with design release V01)
-- *
-- * Copyright 2003 by BKtel communications GmbH
-- *
-- *     V.0.1 07.05.03  M. Heldmann    first draft
-- *     V.0.2 23.07.03  M. Heldmann    added descriptions for the states
-- *     V.0.3 04.10.06  M. Heldmann    NESlotWriteValue expanded due to new 2G6 platform needs
-- *     V.0.4 29.05.08  M. Heldmann    renamed threshold parameters in xxxxMeasuringValuesTable
-- *     V.0.5 11.07.13  M. Heldmann    adjusted syntax of "CommonEntry" to remove MIB compiler warnings
-- *     V.0.6 05.09.14  M. Heldmann    changed CommonModuleWidth and ConfigNESlotWrite STATUS to optional
-- ****************************************************************************

BKTEL-HFC862-OVA-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    ovaV01                 OBJECT IDENTIFIER ::= { modules 104 }
    ovaV01Common           OBJECT IDENTIFIER ::= { ovaV01 1 }
    ovaV01States           OBJECT IDENTIFIER ::= { ovaV01 2 }
    ovaV01Configuration    OBJECT IDENTIFIER ::= { ovaV01 3 }
    ovaV01Control          OBJECT IDENTIFIER ::= { ovaV01 4 }
    ovaV01MeasuringValues  OBJECT IDENTIFIER ::= { ovaV01 5 }
    ovaV01Display          OBJECT IDENTIFIER ::= { ovaV01 6 }


--
-- Type definitions
--


        RegulationMode ::= INTEGER
            {
            regModeConstantOutputPower(1),
            regModeConstantGain(2)
            }

        NESlotWriteValue ::= INTEGER (-1..99)


--*****************************************************************************************
-- OVA ( Optical Amplifier with design release V01 )

--*****************************************************************************************
-- ovaV01Common group

    ovaV01CommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
  ::= { ovaV01Common 1 }

    ovaV01CommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaV01CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OVA-type in the NE"
  ::= { ovaV01Common 2 }

-- table of states entry(ro/rw)

    ovaV01CommonEntry OBJECT-TYPE
        SYNTAX OvaV01CommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { ovaV01NESlot }
  ::= { ovaV01CommonTable 1 }

-- the structure of the entry

    OvaV01CommonEntry ::= SEQUENCE
    {
        ovaV01NESlot                      NESlotValue,
        ovaV01CommonType                  DisplayString,
        ovaV01CommonDescr                 DisplayString,
        ovaV01CommonFirmwareId            DisplayString,
        ovaV01CommonModuleWidth           ModuleWidthValue
    }

    ovaV01NESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { ovaV01CommonEntry 1 }

    ovaV01CommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { ovaV01CommonEntry 2 }

    ovaV01CommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { ovaV01CommonEntry 3 }

    ovaV01CommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { ovaV01CommonEntry 4 }

    ovaV01CommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= {  ovaV01CommonEntry 5  }

--*****************************************************************************************
-- ovaV01MeasuringValues group

-- table of measuring Values

    ovaV01MeasuringValuesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaV01MeasuringValuesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
   ::= { ovaV01MeasuringValues 1 }

-- table of measuring Values entry

    ovaV01MeasuringValuesEntry OBJECT-TYPE
        SYNTAX OvaV01MeasuringValuesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { ovaV01NESlot }
  ::= { ovaV01MeasuringValuesTable 1 }

--  the structure of the entry

    OvaV01MeasuringValuesEntry ::= SEQUENCE
    {
        ovaV01InputPowerLoLo              INTEGER,
        ovaV01InputPowerLo                INTEGER,
        ovaV01InputPowerValue             INTEGER,
        ovaV01InputPowerHi                INTEGER,
        ovaV01InputPowerHiHi              INTEGER,

        ovaV01OutputPowerLoLo             INTEGER,
        ovaV01OutputPowerLo               INTEGER,
        ovaV01OutputPowerValue            INTEGER,
        ovaV01OutputPowerHi               INTEGER,
        ovaV01OutputPowerHiHi             INTEGER,

        ovaV01GainLoLo                    INTEGER,
        ovaV01GainLo                      INTEGER,
        ovaV01GainValue                   INTEGER,
        ovaV01GainHi                      INTEGER,
        ovaV01GainHiHi                    INTEGER,

        ovaV01Laser1CurrentRelLoLo        INTEGER,
        ovaV01Laser1CurrentRelLo          INTEGER,
        ovaV01Laser1CurrentRelValue       INTEGER,
        ovaV01Laser1CurrentRelHi          INTEGER,
        ovaV01Laser1CurrentRelHiHi        INTEGER,

        ovaV01Laser1TecRelLoLo            INTEGER,
        ovaV01Laser1TecRelLo              INTEGER,
        ovaV01Laser1TecRelValue           INTEGER,
        ovaV01Laser1TecRelHi              INTEGER,
        ovaV01Laser1TecRelHiHi            INTEGER,

        ovaV01Laser1TemperatureLoLo       INTEGER,
        ovaV01Laser1TemperatureLo         INTEGER,
        ovaV01Laser1TemperatureValue      INTEGER,
        ovaV01Laser1TemperatureHi         INTEGER,
        ovaV01Laser1TemperatureHiHi       INTEGER,

        ovaV01Laser2CurrentRelLoLo        INTEGER,
        ovaV01Laser2CurrentRelLo          INTEGER,
        ovaV01Laser2CurrentRelValue       INTEGER,
        ovaV01Laser2CurrentRelHi          INTEGER,
        ovaV01Laser2CurrentRelHiHi        INTEGER,

        ovaV01Laser2TecRelLoLo            INTEGER,
        ovaV01Laser2TecRelLo              INTEGER,
        ovaV01Laser2TecRelValue           INTEGER,
        ovaV01Laser2TecRelHi              INTEGER,
        ovaV01Laser2TecRelHiHi            INTEGER,

        ovaV01Laser2TemperatureLoLo       INTEGER,
        ovaV01Laser2TemperatureLo         INTEGER,
        ovaV01Laser2TemperatureValue      INTEGER,
        ovaV01Laser2TemperatureHi         INTEGER,
        ovaV01Laser2TemperatureHiHi       INTEGER,

        ovaV01Plus24VLoLo                 INTEGER,
        ovaV01Plus24VLo                   INTEGER,
        ovaV01Plus24VValue                INTEGER,
        ovaV01Plus24VHi                   INTEGER,
        ovaV01Plus24VHiHi                 INTEGER,

        ovaV01TemperatureLoLo             INTEGER,
        ovaV01TemperatureLo               INTEGER,
        ovaV01TemperatureValue            INTEGER,
        ovaV01TemperatureHi               INTEGER,
        ovaV01TemperatureHiHi             INTEGER
    }

-- *****
    ovaV01InputPowerLoLo OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input power low alarm threshold in steps of 0.1dBm."
    ::= { ovaV01MeasuringValuesEntry 1 }

    ovaV01InputPowerLo OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input power low warning threshold in steps of 0.1dBm."
    ::= { ovaV01MeasuringValuesEntry 2 }

    ovaV01InputPowerValue OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input power in steps of 0.1dBm."
    ::= { ovaV01MeasuringValuesEntry 3 }

