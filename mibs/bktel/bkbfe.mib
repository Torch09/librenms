BKTEL-HFC862-BFE-V01-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    bfe                 OBJECT IDENTIFIER ::= { modules 120 }
    bfeCommon           OBJECT IDENTIFIER ::= { bfe 1 }
    bfeStates           OBJECT IDENTIFIER ::= { bfe 2 }
    bfeConfiguration    OBJECT IDENTIFIER ::= { bfe 3 }
    bfeControl          OBJECT IDENTIFIER ::= { bfe 4 }
    bfeMeasuringValues  OBJECT IDENTIFIER ::= { bfe 5 }
    bfeDisplay          OBJECT IDENTIFIER ::= { bfe 6 }


--
-- Type definitions
--

    RfMode ::= INTEGER
        {
        modeNormal(1),
        modeLowNoise(2),
        modeLowDistortion(3)
        }

    RedundancyMode ::= INTEGER
        {
        modeForceStandby(1),
        modeAlwaysActive(2),
        -- the following modes are only supported if otxdDisplayRedundancySupported == true(1)
        modeNominalMaster(3),
        modeRedundantSlave(4),
        modeNominalRedundantAutomaticBySlot(5)
        }

--  RedundancyMask ::= INTEGER
--      {
--      maskAlarmsOnly(1),
--      maskAlarmsAndWarnings(2),
--      maskSpecialAdjusted(3)           -- This item is NOT writable !!
--      }

    DeviceRedundancyState ::= INTEGER
        {
        stateNominalMaster(1),
        stateRedundantSlave(2)
        }

    DeviceActivityState ::= INTEGER
        {
        standby(1),
        active(2)
        }

    NESlotWriteValue ::= INTEGER (-1..99)


-- MibCompiler defs --
-- $idx$: InternalVoltage
-- $idx$: Equalizer
-- MibCompiler defs --


--*****************************************************************************************
-- bfeCommon group

    bfeCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
  ::= { bfeCommon 1 }

    bfeCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF BfeCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the BFE-type in the NE"
  ::= { bfeCommon 2 }

-- table of states entry(ro/rw)

    bfeCommonEntry OBJECT-TYPE
        SYNTAX BfeCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { bfeNESlot }
  ::= { bfeCommonTable 1 }

-- the structure of the entry

    BfeCommonEntry ::= SEQUENCE
    {
        bfeNESlot                      NESlotValue,
        bfeCommonType                  DisplayString,
        bfeCommonDescr                 DisplayString,
        bfeCommonFirmwareId            DisplayString,
        bfeCommonModuleWidth           ModuleWidthValue
    }

    bfeNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { bfeCommonEntry 1 }

    bfeCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { bfeCommonEntry 2 }

    bfeCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { bfeCommonEntry 3 }

    bfeCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { bfeCommonEntry 4 }

    bfeCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= { bfeCommonEntry 5 }

