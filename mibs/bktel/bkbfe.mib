--*****************************************************************************************
-- Display group

    bfeDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF BfeDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { bfeDisplay 1 }

-- table of configuration entry(ro/rw)

    bfeDisplayEntry OBJECT-TYPE
        SYNTAX BfeDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { bfeNESlot  }
    ::= { bfeDisplayTable 1 }

-- the structure of the entry

    BfeDisplayEntry ::= SEQUENCE
    {
        bfeDisplayRedundancySupported         TruthValue,
        bfeDisplayDeviceRedundancyState       DeviceRedundancyState,
        bfeDisplayDeviceActivity              DeviceActivityState,

        bfeDisplayEqualizer1Frequency         INTEGER,
        bfeDisplayEqualizer2Frequency         INTEGER,
        bfeDisplayEqualizer3Frequency         INTEGER,
        bfeDisplayEqualizer4Frequency         INTEGER,
        bfeDisplayEqualizer5Frequency         INTEGER,
        bfeDisplayEqualizer6Frequency         INTEGER,
        bfeDisplayEqualizer7Frequency         INTEGER,
        bfeDisplayEqualizer8Frequency         INTEGER,

        bfeDisplayInternalVoltage1Supported   TruthValue,
        bfeDisplayInternalVoltage2Supported   TruthValue,
        bfeDisplayInternalVoltage3Supported   TruthValue,
                                                
        bfeDisplayInputVoltageNominal         INTEGER,
        bfeDisplayInternalVoltage1Nominal     INTEGER,
        bfeDisplayInternalVoltage2Nominal     INTEGER,
        bfeDisplayInternalVoltage3Nominal     INTEGER
    }

    bfeDisplayRedundancySupported OBJECT-TYPE
        SYNTAX  TruthValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns true(1) if a the device supports redundancy"
    ::={ bfeDisplayEntry 1 }

    bfeDisplayDeviceRedundancyState OBJECT-TYPE
        SYNTAX  DeviceRedundancyState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the device's redundancy state"
    ::={ bfeDisplayEntry 2 }

    bfeDisplayDeviceActivity OBJECT-TYPE
        SYNTAX  DeviceActivityState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Returns the actual device's activity state"
    ::={ bfeDisplayEntry 3 }

    bfeDisplayEqualizer1Frequency OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Frequency of equalizer no.1"
    ::= { bfeDisplayEntry 4 }

    bfeDisplayEqualizer2Frequency OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Frequency of equalizer no.2"
    ::= { bfeDisplayEntry 5 }

    bfeDisplayEqualizer3Frequency OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
