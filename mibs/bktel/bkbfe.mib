--*****************************************************************************************
--  bfeStates group

-- Note: For every control OId there must be a correspondant alarm OId!

    bfeStatesTable OBJECT-TYPE
        SYNTAX SEQUENCE OF BfeStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { bfeStates 1 }

-- table of states entry(ro/rw)

    bfeStatesEntry OBJECT-TYPE
        SYNTAX BfeStatesEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Alarms for a module."
        INDEX { bfeNESlot }
    ::= { bfeStatesTable 1 }

-- the structure of the entry

    BfeStatesEntry ::= SEQUENCE
    {
        bfeStatesBootloader             PerceivedSeverityValue,
        bfeStatesCommLoss               PerceivedSeverityValue,
        bfeStatesTemperatureLow         PerceivedSeverityValue,
        bfeStatesTemperatureHigh        PerceivedSeverityValue,
        bfeStatesRfOutputPowerLow       PerceivedSeverityValue,
        bfeStatesRfOutputPowerHigh      PerceivedSeverityValue,
        bfeStatesInputVoltageLow        PerceivedSeverityValue,
        bfeStatesInputVoltageHigh       PerceivedSeverityValue,
        bfeStatesInternalVoltage        PerceivedSeverityValue,
        bfeStatesStandby                PerceivedSeverityValue,
        bfeStatesRedundancySwitch       PerceivedSeverityValue,
        -- ** Only for 19 inch devices with supply and fan units
        bfeStatesPowerSupplyLeft        PerceivedSeverityValue,
        bfeStatesPowerSupplyRight       PerceivedSeverityValue,
        bfeStatesFanLeft                PerceivedSeverityValue,
        bfeStatesFanRight               PerceivedSeverityValue
    }

    bfeStatesBootloader OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The device is running in bootloader mode
             without a legal application software."
    ::= { bfeStatesEntry 1 }

    bfeStatesCommLoss OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "The NEC has lost the connection to the device.
             Reason may be a removed or defective device.
             Note that this state is set by the NEC and not
             by the device"
    ::= { bfeStatesEntry 2 }

    bfeStatesTemperatureLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device temperature low"
    ::= { bfeStatesEntry 3 }

    bfeStatesTemperatureHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device temperature high"
    ::= { bfeStatesEntry 4 }

    bfeStatesRfOutputPowerLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "RF output power (total rms) low"
    ::= { bfeStatesEntry 5 }

    bfeStatesRfOutputPowerHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "RF output power (total rms) high"
    ::= { bfeStatesEntry 6 }

    bfeStatesInputVoltageLow OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage low"
    ::= { bfeStatesEntry 7 }

    bfeStatesInputVoltageHigh OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Input voltage high"
    ::= { bfeStatesEntry 8 }

    bfeStatesInternalVoltage OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Internal voltage out of range"
    ::= { bfeStatesEntry 9 }

    bfeStatesStandby OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "Device is sleeping, no rf output"
    ::= { bfeStatesEntry 10 }

    bfeStatesRedundancySwitch OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "This (redundant) device has been activated (redundancy switchover)"
    ::= { bfeStatesEntry 11 }

    bfeStatesPowerSupplyLeft OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The left (looking from front) power supply unit fails.
                     Only valid for devices in 19 inch housing"
    ::= { bfeStatesEntry 12 }

    bfeStatesPowerSupplyRight OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The right (looking from front) power supply unit fails.
                     Only valid for devices in 19 inch housing"
    ::= { bfeStatesEntry 13 }

    bfeStatesFanLeft OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The left (looking from front) fan unit fails.
                     Only valid for devices in 19 inch housing"
    ::= { bfeStatesEntry 14 }

    bfeStatesFanRight OBJECT-TYPE
        SYNTAX PerceivedSeverityValue
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The right (looking from front) fan unit fails.
                     Only valid for devices in 19 inch housing"
    ::= { bfeStatesEntry 15 }

--*****************************************************************************************
--  bfeControl group

  bfeControlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BfeControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
  ::= { bfeControl 1 }

-- table of control entry(ro/rw)

  bfeControlEntry OBJECT-TYPE
    SYNTAX BfeControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "Alarms for a module."
    INDEX { bfeNESlot }
  ::= { bfeControlTable 1 }

-- the structure of the entry

    BfeControlEntry ::= SEQUENCE
    {
        bfeControlReset             TruthValue,
        bfeControlModuleLedBlink    TruthValue
    }

    bfeControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { bfeControlEntry 1 }

    bfeControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "Writing this variable to true(1) lets the device's
        modul LED blink green for 10 seconds.
        Writing this variable to false(2) stbfe blinking at once.
        This variable always returns false(2) on read requests"
    ::= { bfeControlEntry 2 }


--*****************************************************************************************
-- bfeConfiguration group

    bfeConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF BfeConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { bfeConfiguration 1 }

-- table of configuration entry(ro/rw)

    bfeConfigurationEntry OBJECT-TYPE
        SYNTAX BfeConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { bfeNESlot  }
    ::= { bfeConfigurationTable 1 }

-- the structure of the entry

    BfeConfigurationEntry ::= SEQUENCE
    {
        bfeConfigNESlotWrite         NESlotWriteValue,
        bfeConfigRfMode              RfMode,
        bfeConfigRfGain              INTEGER,
        bfeConfigRedundancyMode      RedundancyMode,
        bfeConfigEqualizer1Level     INTEGER,
        bfeConfigEqualizer2Level     INTEGER,
        bfeConfigEqualizer3Level     INTEGER,
        bfeConfigEqualizer4Level     INTEGER,
        bfeConfigEqualizer5Level     INTEGER,
        bfeConfigEqualizer6Level     INTEGER,
        bfeConfigEqualizer7Level     INTEGER,
        bfeConfigEqualizer8Level     INTEGER
    }

    bfeConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { bfeConfigurationEntry 1 }

    bfeConfigRfMode OBJECT-TYPE
        SYNTAX RfMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The RF mode."
    ::= { bfeConfigurationEntry 2 }

    bfeConfigRfGain OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The desired offset from nominal gain in 0.1 dB units."
    ::= { bfeConfigurationEntry 3 }

    bfeConfigRedundancyMode OBJECT-TYPE
        SYNTAX RedundancyMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "The redundancy mode."
    ::= { bfeConfigurationEntry 4 }

    bfeConfigEqualizer1Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.1"
    ::= { bfeConfigurationEntry 5 }

    bfeConfigEqualizer2Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.2"
    ::= { bfeConfigurationEntry 6 }

    bfeConfigEqualizer3Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.3"
    ::= { bfeConfigurationEntry 7 }

    bfeConfigEqualizer4Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.4"
    ::= { bfeConfigurationEntry 8 }

    bfeConfigEqualizer5Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.5"
    ::= { bfeConfigurationEntry 9 }

    bfeConfigEqualizer6Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.6"
    ::= { bfeConfigurationEntry 10 }

    bfeConfigEqualizer7Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.7"
    ::= { bfeConfigurationEntry 11 }

    bfeConfigEqualizer8Level OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Level of equalizer no.8"
    ::= { bfeConfigurationEntry 12 }

