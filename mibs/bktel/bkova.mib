BKTEL-HFC862-OVA-V11-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        experimental, enterprises, TimeTicks, IpAddress, Counter
                 FROM RFC1155-SMI
    OBJECT-TYPE
                 FROM RFC-1212
        TRAP-TYPE
                 FROM RFC-1215
        modules, DisplayString, TruthValue, PerceivedSeverityValue, NESlotValue, ModuleWidthValue
                 FROM   BKTEL-HFC862-BASE-MIB
    ;

    ova                 OBJECT IDENTIFIER ::= { modules 102 }
    ovaCommon           OBJECT IDENTIFIER ::= { ova 1 }
    ovaStates           OBJECT IDENTIFIER ::= { ova 2 }
    ovaConfiguration    OBJECT IDENTIFIER ::= { ova 3 }
    ovaControl          OBJECT IDENTIFIER ::= { ova 4 }
    ovaMeasuringValues  OBJECT IDENTIFIER ::= { ova 5 }
    ovaDisplay          OBJECT IDENTIFIER ::= { ova 6 }


--
-- Type definitions
--


        RegulationMode ::= INTEGER
            {
            regModeConstantOutputPower(1),
            regModeConstantGain(2)
            }

        SbsEvaluationMode ::= INTEGER
            {
            evalModeDefault(1),
            evalModeFullScan(2)
            }

        SbsEvaluationState ::= INTEGER
            {
            evaluationNotSupported(1),
            evaluationDone(2),
            evaluationInProgress(3),
            evaluationFailed(4)
            }

        ExtIOmode ::= INTEGER
            {
            modeActiveLowOutput(1),   
            modeActiveHighOutput(2),  
            modeRedundantMasterIrreversible(3),   
            modeRedundantMasterFallback(4),   
            modeRedundantSlave(5)
            }

        ExtIOmask ::= INTEGER
            {
            maskAlarmsOnly(1),
            maskAlarmsAndWarnings(2),
            maskSpecial(3)           -- This item is NOT writable !!
            }

        NESlotWriteValue ::= INTEGER (-1..99)


--*****************************************************************************************
-- OVA ( Optical Amplifier with design release V11 )

--*****************************************************************************************
-- ovaCommon group

    ovaCommonNumberOfModules OBJECT-TYPE
        SYNTAX  INTEGER(0..50)
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION "Number of modules in table."
  ::= { ovaCommon 1 }

    ovaCommonTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "The table contains all modules of the OVA-type in the NE"
  ::= { ovaCommon 2 }

-- table of states entry(ro/rw)

    ovaCommonEntry OBJECT-TYPE
        SYNTAX OvaCommonEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION "Common-Values for a module."
        INDEX { ovaNESlot }
  ::= { ovaCommonTable 1 }

-- the structure of the entry

    OvaCommonEntry ::= SEQUENCE
    {
        ovaNESlot                      NESlotValue,
        ovaCommonType                  DisplayString,
        ovaCommonDescr                 DisplayString,
        ovaCommonFirmwareId            DisplayString,
        ovaCommonModuleWidth           ModuleWidthValue
    }

    ovaNESlot OBJECT-TYPE
        SYNTAX  NESlotValue
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The slot number of the chassis for which this
        entry contains management information."
    ::= { ovaCommonEntry 1 }

    ovaCommonType OBJECT-TYPE
        SYNTAX  DisplayString  (SIZE(0..32))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "The type of physical module. modSlotEmpty indicates
         an empty slot.  A Value of modSlotUnknown indicates
         that the type of module is unknown."
    ::= { ovaCommonEntry 2 }

    ovaCommonDescr OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "A textual description of the module.
        If not available, this Value should be
        set to a zero length string."
    ::= { ovaCommonEntry 3 }

    ovaCommonFirmwareId OBJECT-TYPE
        SYNTAX DisplayString (SIZE(0..32))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION "The firmware Id of the module."
    ::= { ovaCommonEntry 4 }

    ovaCommonModuleWidth OBJECT-TYPE
        SYNTAX ModuleWidthValue
        ACCESS read-only
        STATUS optional
        DESCRIPTION "The width of the module in multiples of slots (1, 2, ...)"
    ::= { ovaCommonEntry 5 }

