--*****************************************************************************************
--  ovaControl group

  ovaControlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF OvaControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION ""
  ::= { ovaControl 1 }

-- table of control entry(ro/rw)

  ovaControlEntry OBJECT-TYPE
    SYNTAX OvaControlEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION "Alarms for a module."
    INDEX { ovaNESlot }
  ::= { ovaControlTable 1 }

-- the structure of the entry

    OvaControlEntry ::= SEQUENCE
    {
        ovaControlLaserShutdown                     TruthValue,
        ovaControlReset                             TruthValue,
        ovaControlModuleLedBlink                    TruthValue,
        ovaControlSbsEvaluationStart                TruthValue
    }

    ovaControlLaserShutdown OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION "Shutdown the laser(s), optical power on/off."
    ::= { ovaControlEntry 1 }

    ovaControlReset OBJECT-TYPE
      SYNTAX TruthValue
      ACCESS read-write
      STATUS mandatory
      DESCRIPTION "Reset the module."
    ::= { ovaControlEntry 2 }

    ovaControlModuleLedBlink OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "Writing this variable to true(1) lets the device's 
        modul LED blink green for 10 seconds. 
        Writing this variable to false(2) stops blinking at once.
        This variable always returns false(2) on read requests"
    ::= { ovaControlEntry 3 }

    ovaControlSbsEvaluationStart OBJECT-TYPE
        SYNTAX TruthValue
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Setting to true(1) starts an SBS threshold evaluation process.
             Always reads as false(2)"
    ::={ ovaControlEntry 4 }


--*****************************************************************************************
-- ovaConfiguration group

    ovaConfigurationTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ovaConfiguration 1 }

-- table of configuration entry(ro/rw)

    ovaConfigurationEntry OBJECT-TYPE
        SYNTAX OvaConfigurationEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ovaNESlot  }
    ::= { ovaConfigurationTable 1 }

-- the structure of the entry

    OvaConfigurationEntry ::= SEQUENCE
    {
        ovaConfigNESlotWrite                     NESlotWriteValue,
        ovaConfigRegulationMode                  RegulationMode,
        ovaConfigOutputPwrOrGainAdjust           INTEGER,
        ovaConfigInputPwrLimitLoLo               INTEGER,
        ovaConfigInputPwrLimitLo                 INTEGER,
        ovaConfigInputPwrLimitHi                 INTEGER,
        ovaConfigInputPwrLimitHiHi               INTEGER,
        ovaConfigOutputPwrOrGainLimitLoLo        INTEGER,
        ovaConfigOutputPwrOrGainLimitLo          INTEGER,
        ovaConfigOutputPwrOrGainLimitHi          INTEGER,
        ovaConfigOutputPwrOrGainLimitHiHi        INTEGER,
        ovaConfigModeExtIO                       ExtIOmode,
        ovaConfigMaskExtIO                       ExtIOmask,
        ovaConfigSbsEvaluationMode               SbsEvaluationMode
    }

    ovaConfigNESlotWrite OBJECT-TYPE
        SYNTAX NESlotWriteValue
        ACCESS read-write
        STATUS optional
        DESCRIPTION "By writing this variable a slot can be assigned
            for devices that dont support hardware slot detection.
            Reading '-1' means that the slot position is NOT writable."
    ::= { ovaConfigurationEntry 1 }

    ovaConfigRegulationMode OBJECT-TYPE
        SYNTAX  RegulationMode
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The regulation mode of the device.
             - regModeConstantOutputPower(1):
               If the device runs in constant output power mode, then all *OutputPwrOrGain*
               variables are interpreted as 'Output Power' specific.
             - regModeConstantGain(2):
               If the device runs in constant gain mode, then all *OutputPwrOrGain*
               variables are interpreted as 'Gain' specific."
    ::={ ovaConfigurationEntry 2 }

    ovaConfigOutputPwrOrGainAdjust OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "This variable can be used to adjust the desired output power (or gain) 
             value in 0.1 dB(m) units.
             See ovaConfigRegulationMode variable too"
    ::={ ovaConfigurationEntry 3 }

    ovaConfigInputPwrLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power low alarm limit in 0.1 dBm units"
    ::={ ovaConfigurationEntry 4 }

    ovaConfigInputPwrLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power low warning limit in 0.1 dBm units"
    ::={ ovaConfigurationEntry 5 }

    ovaConfigInputPwrLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power high warning limit in 0.1 dBm units"
    ::={ ovaConfigurationEntry 6 }

    ovaConfigInputPwrLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The optical input power high alarm limit in 0.1 dBm units"
    ::={ ovaConfigurationEntry 7 }

    ovaConfigOutputPwrOrGainLimitLoLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power (or gain) low alarm limit in 0.1 dBm units (relative to adjusted output power or gain)"
    ::={ ovaConfigurationEntry 8 }

    ovaConfigOutputPwrOrGainLimitLo OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power (or gain) low warning limit in 0.1 dBm units (relative to adjusted output power or gain)"
    ::={ ovaConfigurationEntry 9 }

    ovaConfigOutputPwrOrGainLimitHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power (or gain) high warning limit in 0.1 dBm units (relative to adjusted output power or gain)"
    ::={ ovaConfigurationEntry 10 }

    ovaConfigOutputPwrOrGainLimitHiHi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "The output power (or gain) high alarm limit in 0.1 dBm units (relative to adjusted output power or gain)"
    ::={ ovaConfigurationEntry 11 }

    ovaConfigModeExtIO OBJECT-TYPE
        SYNTAX ExtIOmode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   "The mode and usage of the device's external IO port."
    ::= { ovaConfigurationEntry 12 }

    ovaConfigMaskExtIO OBJECT-TYPE
        SYNTAX ExtIOmask
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION   
          "The alarm/warning mask that activates the device's external IO port."
    ::= { ovaConfigurationEntry 13 }

    ovaConfigSbsEvaluationMode OBJECT-TYPE
        SYNTAX SbsEvaluationMode
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "The SBS threshold evaluation mode."
    ::={ ovaConfigurationEntry 14 }

--*****************************************************************************************
-- Display group

    ovaDisplayTable OBJECT-TYPE
        SYNTAX SEQUENCE OF OvaDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
    ::= { ovaDisplay 1 }

-- table of configuration entry(ro/rw)

    ovaDisplayEntry OBJECT-TYPE
        SYNTAX OvaDisplayEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION ""
        INDEX { ovaNESlot  }
    ::= { ovaDisplayTable 1 }

-- the structure of the entry

    OvaDisplayEntry ::= SEQUENCE
    {
        ovaDisplayLaserCount                INTEGER,
        ovaDisplayOutputPwrOrGainAdjustMin  INTEGER,
        ovaDisplayOutputPwrOrGainAdjustMax  INTEGER,
        ovaDisplayOutputPwrOrGainAdjustStep INTEGER,
        ovaDisplayLaser1PumpPowerRel        INTEGER,
        ovaDisplayLaser2PumpPowerRel        INTEGER,
        ovaDisplayOutputPwrOrGainNominal    INTEGER,
        ovaDisplaySbsEvaluatedThreshold     INTEGER,
        ovaDisplaySbsLastEvaluationState    SbsEvaluationState,
        ovaDisplaySbsLastEvaluationTime     DisplayString
    }

    ovaDisplayLaserCount OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The number of lasers used by this optical amplifier"
    ::={ ovaDisplayEntry 1 }

    ovaDisplayOutputPwrOrGainAdjustMin OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The minimum adjustable offset from nominal output power (or gain) in 0.1 dB(m) units.
             See ovaConfigRegulationMode variable too"
    ::={ ovaDisplayEntry 2 }

    ovaDisplayOutputPwrOrGainAdjustMax OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The maximum adjustable offset from nominal output power (or gain) in 0.1 dB(m) units.
             See ovaConfigRegulationMode variable too"
    ::={ ovaDisplayEntry 3 }

    ovaDisplayOutputPwrOrGainAdjustStep OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The stepsize that can be used to adjust the output power (or gain) in 0.1 dB units.
             See ovaConfigRegulationMode variable too"
    ::={ ovaDisplayEntry 4 }

    ovaDisplayLaser1PumpPowerRel OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The pump power of laser no.1 in percent"
    ::={ ovaDisplayEntry 5 }

    ovaDisplayLaser2PumpPowerRel OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The pump power of laser no.2 in percent"
    ::={ ovaDisplayEntry 6 }

    ovaDisplayOutputPwrOrGainNominal OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The nominal output power (or gain) in 0.1 dB(m) units.
             See ovaConfigRegulationMode variable too"
    ::={ ovaDisplayEntry 7 }

    ovaDisplaySbsEvaluatedThreshold OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The evaluated SBS output power or gain threshold in 0.1 dB(m) units.
             Not supported by all optical amplifiers"
    ::={ ovaDisplayEntry 8 }

    ovaDisplaySbsLastEvaluationState OBJECT-TYPE
        SYNTAX  SbsEvaluationState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The result of last SBS threshold evaluation process.
             Returns 'evaluationNotSupported(1)' if SBS threshold evaluation 
             is not supported by this optical amplifier"
    ::={ ovaDisplayEntry 9 }

    ovaDisplaySbsLastEvaluationTime OBJECT-TYPE
        SYNTAX  DisplayString
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The time of last successfull SBS threshold evaluation process.
             Not supported by all optical amplifiers"
    ::={ ovaDisplayEntry 10 }


